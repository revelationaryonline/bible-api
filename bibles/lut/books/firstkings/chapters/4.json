[
  {
    "id": "1138135094132211714",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "1",
    "text": "Also war Salomo Konig uber ganz Israel.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138135094655123458",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "2",
    "text": "Und dies waren seine Fursten: Asarja, der Sohn Zadoks, des Priesters,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138135095171547138",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "3",
    "text": "Elihoreph und Ahija, die Sohne Sisas, waren Schreiber. Josaphat, der Sohn Ahiluds, war Kanzler.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138135095700226050",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "4",
    "text": "Benaja der Sohn Jojadas, war Feldhauptmann. Zadok und Abjathar waren Priester.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138135096237522946",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "5",
    "text": "Asarja, der Sohn Nathans, war über die Amtleute. Sabud, der Sohn Nathans, des Priesters, war des Konigs Freund.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138135096771117058",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "6",
    "text": "Ahisar war Hofmeister: Adoniram, der Sohn Abdas, war Rentmeister.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138135097313198082",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "7",
    "text": "Und Salomo hatte zwolf Amtleute über ganz Israel, die den Konig und sein Haus versorgten. Einer hatte des Jahrs einen Monden lang zu versorgen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138135097840959490",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "8",
    "text": "Und hieften also: Der Sohn Hurs auf dem Gebirge Ephraim;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138135098386153474",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "9",
    "text": "der Sohn Dekers zu Makaz und zu Saalbim und zu Beth-Semes und zu Elon und Beth-Hanan;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138135098909196290",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "10",
    "text": "der Sohn Heseds zu Aruboth, und hatte dazu Socho und das ganze Land Hepher;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138135099449475074",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "11",
    "text": "der Sohn Abinadabs: die ganze Herrschaft zu Dor; und hatte Taphath, Salomos Tochter, zum Weibe;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138135100002009090",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "12",
    "text": "Baena, der Sohn Ahiluds, zu Thaenach und zu Megiddo und über ganz Beth-Sean, welches liegt neben Zarthana unter Jesreel, von Beth-Sean bis an den Plan Mehola, bisjenseit Jakmeam;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138135100534915074",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "13",
    "text": "der Sohn Gebers zu Ramoth in Gilead, und hatte die Flecken Jairs, des Sohns Manasses, in Gilead, und hatte die Gegend Argob, die in Basan liegt, sechzig grafter Stadte, vermauert und mit ehernen Riegeln;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138135101060055042",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "14",
    "text": "Ahinadab, der Sohn Iddos, zu Mahanaim;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138135101576347650",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "15",
    "text": "Ahimaaz in Naphthali; und der nahm auch Salomos Tochter, Basmath, zum Weibe;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138135102161846274",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "16",
    "text": "Baena, der Sohn Husais, in Asser und zu Aloth;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138135102719655938",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "17",
    "text": "Josaphat, der Sohn Paruahs, in Isaschar;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138135103281823746",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "18",
    "text": "Simei, der Sohn Elas, in Benjamin;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138135103831965698",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "19",
    "text": "Geber, der Sohn Uris, im Lande Gilead, im Lande Sihons, des Konigs der Amoriter und Ogs, des Konigs in Basan; ein Amtmann war in demselben Lande.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138135104366018562",
    "book": "firstkings                         ",
    "chapter": "4",
    "verse": "20",
    "text": "Juda aber und Israel, des war viel, wie der Sand am Meer; und alien und tranken und waren frohlich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  }
]