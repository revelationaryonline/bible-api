[
  {
    "id": "1138143914932699138",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "1",
    "text": "Dies sind die Worte des Konigs Lamuel, die Lehre, die ihn seine Mutter lehrete:                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138143915483889666",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "2",
    "text": "Ach, mein Auserwahlter, ach, du Sohn meines Leibes, ach, mein gewünschter Sohn,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138143916044681218",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "3",
    "text": "la ft nicht den Weibern dein Vermogen und gehe die Wege nicht, darin sich die Konige verderben!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138143916610256898",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "4",
    "text": "0, nicht den Konigen, Lamuel, gib den Konigen nicht Wein zu trinken noch den Fursten stark Getranke.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138143917161152514",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "5",
    "text": "Sie mochten trinken und der Rechte vergessen und verandern die Sache irgend der elenden Leute.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138143917737345026",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "6",
    "text": "Gebet stark Getranke denen, die umkommen sollen, und den Wein den betrubten Seelen,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138143918285357058",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "7",
    "text": "daß sie trinken und ihres Elendes vergessen und ihres Unglucks nicht mehr gedenken.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138143918831894530",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "8",
    "text": "Tu deinen Mund auf fur die Stummen und fur die Sache alter, die verlassen sind.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138143919407038466",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "9",
    "text": "Tu deinen Mund auf und richte recht und rache den Elenden und Armen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138143919955148802",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "10",
    "text": "Wem ein tugendsam Weib bescheret ist, die ist viel edler denn die kostlichsten Perlen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143920506142722",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "11",
    "text": "Ihres Mannes Herzdarf sich auf sie verlassen, und Nahrung wird ihm nicht mangeln.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138143921048813570",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "12",
    "text": "Sie tut ihm Liebes und kein Leides sein Leben lang.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138143921586634754",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "13",
    "text": "Sie gehet mit Wolle und Flachs um und arbeitet gerne mitihren Handen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138143922137464834",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "14",
    "text": "Sie istwie ein Kaufmannsschiff, das seine Nahrung von feme bringt.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138143922692227074",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "15",
    "text": "Sie stehet des Nachts auf und gibt Futter ihrem Flause und Essen ihren Dirnen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138143923242795010",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "16",
    "text": "Sie denkt nach einem Acker und kauft ihn und pflanzt einen Weinberg von den Fruchten ihrer FHande.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138143923804602370",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "17",
    "text": "Sie gurtet ihre Lenden fest und starkt ihre Arme.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138143924362575874",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "18",
    "text": "Sie merkt, wie ihr Handel Frommen bringt; ihre Leuchte verloscht des Nachts nicht.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138143924929953794",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "19",
    "text": "Sie streckt ihre Hand nach dem Rocken, und ihre Finger fassen die Spindel.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138143925491335170",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "20",
    "text": "Sie breitet ihre Hande aus zu dem Armen und reichet ihre Hand dem Durftigen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138143926041804802",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "21",
    "text": "Sie furchtet ihres Hauses nicht vor dem Schnee, denn ihr ganzes Haus hat zwiefache Kleider.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138143926561570818",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "22",
    "text": "Sie macht ihr selbst Decken; weifte Seide und Purpur ist ihr Kleid.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138143927104077826",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "23",
    "text": "Ihr Mann ist berühmt in den Toren, wenn er sitzt bei den Altesten des Landes.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138143927654121474",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "24",
    "text": "Sie macht einen Rock und verkauft ihn; einen Gürtel gibt sie dem Kramer.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138143928229036034",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "25",
    "text": "Ihr Schmuck ist, daß sie reinlich und fleiftig ist; und wird hernach lachen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138143928805392386",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "26",
    "text": "Sie tut ihren Mund auf mit Weisheit, und auf ihrer Zunge ist holdselige Lehre.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138143929357172738",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "27",
    "text": "Sie schauet, wie es in ihrem Hause zugehet, und isset ihr Brot nicht mit Faulheit.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138143929928318978",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "28",
    "text": "Ihre Sohne kommen auf und preisen sie selig; ihr Mann lobt sie.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138143930468433922",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "29",
    "text": "Viele Tochter bringen Reichtum; du aber übertriffst sie alle.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138143931022114818",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "30",
    "text": "Lieblich und schon sein ist nichts; ein Weib, das den HERRN furchtet, soil man loben.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138143931533099010",
    "book": "proverbs                           ",
    "chapter": "31",
    "verse": "31",
    "text": "Sie wird gerühmt werden von den Fruchten ihrer Hande; und ihre Werke werden sie loben in den Toren.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  }
]