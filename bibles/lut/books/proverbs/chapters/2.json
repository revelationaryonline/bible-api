[
  {
    "id": "1138143443655950338",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "1",
    "text": "Mein Kind, willst du meine Rede annehmen und meine Gebote bei dir behalten,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138143444213432322",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "2",
    "text": "so laft dein Ohr auf Weisheit achthaben und neige dein Herz mit Fleifi dazu.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138143444777304066",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "3",
    "text": "Denn so du mit Fleifi danach rufest und darum betest,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138143445322924034",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "4",
    "text": "so du sie suchest wie Silber und forschest sie wie die Schatze,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138143445878046722",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "5",
    "text": "alsdann wirst du die Furcht des FIERRN vernehmen und Gottes Erkenntnis finden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138143446415540226",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "6",
    "text": "Denn der HERR gibt Weisheit, und aus seinem Munde kommt Erkenntnis und Verstand.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138143446971285506",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "7",
    "text": "Er last's den Aufrichtigen gelingen und beschirmet die Frommen                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138143447526735874",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "8",
    "text": "und behutet die, so recht tun, und bewahret den Weg seiner Heiligen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138143448075108354",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "9",
    "text": "Dann wirst du verstehen Gerechtigkeit und Recht und Frommigkeit und alien guten Weg.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138143448628723714",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "10",
    "text": "Wo die Weisheit dir zu Herzen gehet, dafi du gerne lernest,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138143449179324418",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "11",
    "text": "so wird dich guter Rat bewahren und Verstand wird dich behuten,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138143449750536194",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "12",
    "text": "daß du nicht geratest auf den Weg der Bosen noch unter die verkehrten Schwatzer,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138143450314506242",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "13",
    "text": "die da verlassen die rechte Bahn und gehen finstere Wege,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138143450867171330",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "14",
    "text": "die sich freuen, Boses zu tun, und sind frohlich in ihrem bosen, verkehrten Wesen,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138143451421114370",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "15",
    "text": "welche ihren Weg verkehren und folgen ihrem Abwege;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138143452014739458",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "16",
    "text": "daß du nicht geratest an eines andern Weib, und die nicht dein ist, die glatte Worte gibt                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138143452568092674",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "17",
    "text": "und verlafttden Herrn ihrer Jugend und vergisset den Bund ihres Gottes;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138143453116137474",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "18",
    "text": "denn ihr Haus neigetsich zum Tode und ihre Gange zu den Verlornen;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138143453669523458",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "19",
    "text": "alle, die zu ihr eingehen, kommen nicht wieder und ergreifen den Weg des Lebens nicht:                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143454230282242",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "20",
    "text": "auf daß du wandelst auf gutem Wege und bleibest auf der rechten Bahn.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143454772854786",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "21",
    "text": "Denn die Gerechten werden im Lande wohnen, und die Frommen werden drinnen bleiben;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138143455325650946",
    "book": "proverbs                           ",
    "chapter": "2",
    "verse": "22",
    "text": "aber die Gottlosen werden aus dem Lande gerottet, und die Verachter werden draus vertilget.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  }
]