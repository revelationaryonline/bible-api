[
  {
    "id": "1138143522410856450",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "1",
    "text": "Mein Kind, behalte meine Rede und verbirg meine Gebote bei dir!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138143522955264002",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "2",
    "text": "Behalte meine Gebote, so wirst du leben, und mein Gesetz wie deinen Augapfel.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138143523528441858",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "3",
    "text": "Binde sie an deine Finger, schreibe sie auf die Tafel deines Herzens!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138143524066295810",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "4",
    "text": "Sprich zur Weisheit: Du bist meine Schwester, und nenne die Klugheit deine Freundin,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138143524626169858",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "5",
    "text": "daß du behutet werdest vor dem fremden Weibe, vor einer andern, die glatte Worte gibt.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138143525179195394",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "6",
    "text": "Denn am Fenster meines Hauses guckte ich durchs Gitter und sah unter den Albernen                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138143525716328450",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "7",
    "text": "und ward gewahr unter den Kindern eines narrischen Junglings,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138143526269747202",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "8",
    "text": "der ging auf der Gasse an einer Ecke und trat daher auf dem Wege an ihrem Hause,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138143526825525250",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "9",
    "text": "in der Dammerung, am Abend des Tages, da es Nacht ward und dunkel war.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143527379140610",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "10",
    "text": "Und siehe, da begegnete ihm ein Weib im Hurenschmuck, listig,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138143527985905666",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "11",
    "text": "wild und unbandig, daß ihre Fuße in ihrem Hause nicht bleiben konnen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138143528530804738",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "12",
    "text": "Jetzt ist sie draulien, jetzt auf der Gasse und lauert an alien Ecken.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143529086746626",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "13",
    "text": "Und erwischte ihn und kussete ihn unverschamt und sprach zu ihm:                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138143529647374338",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "14",
    "text": "Ich habe Dankopfer fur mich heute bezahlet, fur meine Geiubde.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138143530199056386",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "15",
    "text": "Darum bin ich herausgegangen, dirzu begegnen, dein Angesicht fruhe zu suchen, und habe dich funden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138143530742317058",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "16",
    "text": "Ich habe mein Bett schon geschmückt mit bunten Teppichen aus Agypten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143531266834434",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "17",
    "text": "Ich habe mein Lager mit Myrrhen, Aloes und Zinnamen besprengt.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138143531826249730",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "18",
    "text": "Komm, lali uns genug buhlen bis an den Morgen und lali uns der Liebe pflegen;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138143532375015426",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "19",
    "text": "denn der Mann ist nicht daheim, er isteinen fernen Weg gezogen;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138143532917686274",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "20",
    "text": "er hat den Geldsack mit sich genommen; er wird erst aufs Fest wieder heimkommen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138143533463240706",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "21",
    "text": "Sie uberredete ihn mit vielen Worten und gewann ihn ein mit ihrem glatten Munde.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138143533997588482",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "22",
    "text": "Er folgte ihr bald nach; wie ein Ochs zur Fleischbank gefuhrt wird, und wie zur Fessel, da man die Narren zuchtiget,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138143534543929346",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "23",
    "text": "bis sie ihm mit dem Pfeil die Leber spaltete, wie ein Vogel zum Strick eilet und weili nicht, daß ihm das Leben gilt.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143535091089410",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "24",
    "text": "So gehorchet mir nun, meine Kinder, und merket auf die Rede meines Mundes.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138143535646769154",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "25",
    "text": "Lali dein Herz nicht weichen auf ihren Weg und lali dich nicht verführen auf ihre Bahn!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138143536245211138",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "26",
    "text": "Denn sie hat viele verwundet und gefallet, und sind allerlei Machtige von ihr erwurget.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138143536782245890",
    "book": "proverbs                           ",
    "chapter": "7",
    "verse": "27",
    "text": "Ihr Haus sind Wege zur Holle, da man hinunterfahrt in des Todes Kammer.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  }
]