[
  {
    "id": "1138142996666777602",
    "book": "psalms                             ",
    "chapter": "140",
    "verse": "1",
    "text": "Ein Psalm Davids, vorzusingen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142997215772674",
    "book": "psalms                             ",
    "chapter": "140",
    "verse": "2",
    "text": "Errette mich, HERR, von den bosen Menschen; behüte mich vor den freveln Leuten,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138142997757591554",
    "book": "psalms                             ",
    "chapter": "140",
    "verse": "3",
    "text": "die Boses gedenken in ihrem Herzen und taglich Krieg erregen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138142998339059714",
    "book": "psalms                             ",
    "chapter": "140",
    "verse": "4",
    "text": "Sie scharfen ihre Zunge wie eine Schlange; Otterngift ist unter ihren Lippen. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138142998890840066",
    "book": "psalms                             ",
    "chapter": "140",
    "verse": "5",
    "text": "Bewahre mich, HERR, vorder Hand der Gottlosen; behüte mich vor den freveln Leuten, die meinen Gang gedenken umzustoften.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138142999438589954",
    "book": "psalms                             ",
    "chapter": "140",
    "verse": "6",
    "text": "Die Hoffartigen legen mir Stricke und breiten mir Seile aus zum Netz und stellen mir Fallen an den Weg. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138142999994302466",
    "book": "psalms                             ",
    "chapter": "140",
    "verse": "7",
    "text": "Ich aber sage zum HERRN: Du bist mein Gott; HERR, vernimm die Stimme meines Flehens!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138143000551620610",
    "book": "psalms                             ",
    "chapter": "140",
    "verse": "8",
    "text": "HERR Herr, meine starke Hilfe, du beschirmest mein Haupt zur Zeit des Streits.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138143001080365058",
    "book": "psalms                             ",
    "chapter": "140",
    "verse": "9",
    "text": "HERR, laft dem Gottlosen seine Begierde nicht; starke seinen Mutwillen nicht; sie mochten sich's erheben. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138143001641156610",
    "book": "psalms                             ",
    "chapter": "140",
    "verse": "10",
    "text": "Das Ungluck, davon meine Feinde ratschlagen, müsse auf ihren Kopf fallen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138143002205880322",
    "book": "psalms                             ",
    "chapter": "140",
    "verse": "11",
    "text": "Erwird Strahlen über sie schütten; erwird sie mit Feuer tief in die Erde schlagen, daß sie nimmer nicht aufstehen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138143002750517250",
    "book": "psalms                             ",
    "chapter": "140",
    "verse": "12",
    "text": "Ein boses Maul wird kein Gluck haben auf Erden; ein frevelboser Mensch wird verjagt und gestürzt werden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138143003295318018",
    "book": "psalms                             ",
    "chapter": "140",
    "verse": "13",
    "text": "Denn ich weift, dafi der HERR wird des Elenden Sache und der Armen Recht ausfuhren.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138143003833630722",
    "book": "psalms                             ",
    "chapter": "140",
    "verse": "14",
    "text": "Auch werden die Gerechten deinem Namen danken, und die Frommen werden vor deinem Angesichte bleiben.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  }
]