[
  {
    "id": "1138141589174878210",
    "book": "psalms                             ",
    "chapter": "20",
    "verse": "1",
    "text": "Ein Psalm Davids, vorzusingen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141589724332034",
    "book": "psalms                             ",
    "chapter": "20",
    "verse": "2",
    "text": "Der HERR erhore dich in der Not; der Name des Gottes Jakobs schutze dich!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141590269984770",
    "book": "psalms                             ",
    "chapter": "20",
    "verse": "3",
    "text": "Er sende dir Hilfe vom Heiligtum und starke dich aus Zion!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138141590816653314",
    "book": "psalms                             ",
    "chapter": "20",
    "verse": "4",
    "text": "Er gedenke all deines Speisopfers, und dein Brandopfer musse fett sein. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138141591363158018",
    "book": "psalms                             ",
    "chapter": "20",
    "verse": "5",
    "text": "Er gebe dir, was dein Herz begehret, und erfülle alle deine Anschlage!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138141591947640834",
    "book": "psalms                             ",
    "chapter": "20",
    "verse": "6",
    "text": "Wir rühmen, daß du uns hilfst; und im Namen unsers Gottes werfen wir Panier auf. Der HERR gewahre dir alle deine Bitte!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141592539103234",
    "book": "psalms                             ",
    "chapter": "20",
    "verse": "7",
    "text": "Nun merke ich, daß der HERR seinem Gesalbten hilft und erhoret ihn in seinem heiligen Himmel; seine rechte Hand hilft gewaltiglich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141593083969538",
    "book": "psalms                             ",
    "chapter": "20",
    "verse": "8",
    "text": "Jene verlassen sich auf Wagen und Rosse; wir aber denken an den Namen des HERRN, unsers Gottes.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138141593625919490",
    "book": "psalms                             ",
    "chapter": "20",
    "verse": "9",
    "text": "Sie sind niedergesturzt und gefallen; wir aber stehen aufgerichtet.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141594175111170",
    "book": "psalms                             ",
    "chapter": "20",
    "verse": "10",
    "text": "Hilf, HERR! Der Konig erhore uns, wenn wir rufen!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  }
]