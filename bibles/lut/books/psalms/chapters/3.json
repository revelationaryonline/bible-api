[
  {
    "id": "1138141459254214658",
    "book": "psalms                             ",
    "chapter": "3",
    "verse": "1",
    "text": "Ein Psalm Davids, da er floh vor seinem Sohn Absalom.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138141459790364674",
    "book": "psalms                             ",
    "chapter": "3",
    "verse": "2",
    "text": "Ach, HERR, wie ist meiner Feinde so viel, und setzen sich so viele wider mich!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141460373602306",
    "book": "psalms                             ",
    "chapter": "3",
    "verse": "3",
    "text": "Viele sagen von meiner Seele: Sie hat keine Hilfe bei Gott. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141460904345602",
    "book": "psalms                             ",
    "chapter": "3",
    "verse": "4",
    "text": "Aber du, HERR, bist der Schild fur mich, und der mich zu Ehren setzet und mein Haupt aufrichtet.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138141461458354178",
    "book": "psalms                             ",
    "chapter": "3",
    "verse": "5",
    "text": "Ich rufe an mit meiner Stimme den HERRN, so erhoret er mich von seinem heiligen Berge. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141462006890498",
    "book": "psalms                             ",
    "chapter": "3",
    "verse": "6",
    "text": "Ich liege und schlafe und erwache; denn der HERR halt mich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141462569680898",
    "book": "psalms                             ",
    "chapter": "3",
    "verse": "7",
    "text": "Ich furchte mich nicht vor viel Hunderttausenden, die sich umher wider mich legen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141463108943874",
    "book": "psalms                             ",
    "chapter": "3",
    "verse": "8",
    "text": "Auf HERR, und hilf mir, mein Gott; denn du schlagst alle meine Feinde auf den Backen und zerschmetterst der Gottlosen Zahne.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141463681335298",
    "book": "psalms                             ",
    "chapter": "3",
    "verse": "9",
    "text": "Bei dem HERRN findet man Hilfe und deinen Segen uber dein Volk. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  }
]