[
  {
    "id": "1138142777148440578",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "1",
    "text": "Danketdem HERRN; denn eristfreundlich, und seine Gute wahret ewiglich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142777715195906",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "2",
    "text": "Es sage nun Israel: Seine Gute wahret ewiglich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138142778247217154",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "3",
    "text": "Es sage nun das Haus Aaron: Seine Gute wahret ewiglich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138142778794704898",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "4",
    "text": "Es sagen nun, die den HERRN furchten: Seine Gute wahret ewiglich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142779324170242",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "5",
    "text": "In der Angst rief ich den HERRN an, und der HERR erhorete mich und trostete mich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142779885453314",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "6",
    "text": "Der HERR ist mit mir, darum furchte ich mich nicht; was konnen mir Menschen tun?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138142780422619138",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "7",
    "text": "Der HERR ist mit mir, mirzu helfen; und ich will meine Lust sehen an meinen Feinden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138142780975644674",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "8",
    "text": "Es ist gut auf den HERRN vertrauen und sich nicht verlassen auf Menschen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138142781541089282",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "9",
    "text": "Es ist gut auf den HERRN vertrauen und sich nicht verlassen auf Fursten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142782061051906",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "10",
    "text": "Alle Heiden umgeben mich; aber im Namen des HERRN will ich sie zerhauen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142782597234690",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "11",
    "text": "Sie umgeben mich allenthalben ; aber im Namen des HERRN will ich sie zerhauen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142783163564034",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "12",
    "text": "Sie umgeben mich wie Bienen, siedampfen wie ein Feuer in Dornen; aber im Namen des HERRN will ich sie zerhauen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138142783729827842",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "13",
    "text": "Man stofiet mich, daß ich fallen soil; aber der HERR hilft mir.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138142784271679490",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "14",
    "text": "Der HERR ist meine Macht und mein Psalm und ist mein Heil.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138142784809697282",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "15",
    "text": "Man singt mit Freuden vom Sieg in den Hütten der Gerechten. Die Rechte des HERRN behalt den Sieg;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138142785395752962",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "16",
    "text": "die Rechte des HERRN isterhohet; die Rechte des HERRN behalt den Sieg.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142785943732226",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "17",
    "text": "Ich werde nicht sterben, sondern ieben und des HERRN Werk verkundigen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142786455273474",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "18",
    "text": "Der HERR zuchtiget mich wohl, aberergibt mich dem Tode nicht.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138142787002826754",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "19",
    "text": "Tut mir auf die Tore der Gerechtigkeit, dafi ich dahin eingehe und dem HERRN danke.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138142787560898562",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "20",
    "text": "Das ist das Tor des HERRN; die Gerechten werden dahin eingehen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138142788138270722",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "21",
    "text": "Ich danke dir, daß du mich demutigest und hilfst mir.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142788698505218",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "22",
    "text": "Der Stein, den die Bauleute verworfen, ist zum Eckstein worden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138142789239832578",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "23",
    "text": "Das ist vom HERRN geschehen und ist ein Wunder vor unsern Augen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138142789794988034",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "24",
    "text": "Dies ist der Tag, den der HERR macht; lalit uns freuen und frohlich drinnen sein!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142790329597954",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "25",
    "text": "0 HERR, hilf, o HERR, lafi wohlgelingen!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142790874660866",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "26",
    "text": "Gelobet sei, der da kommt im Namen des HERRN! Wir segnen euch, die ihr vom Hause des HERRN seid.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138142791455178754",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "27",
    "text": "Der HERR ist Gott, der uns erleuchtet. Schmucket das Fest mit Maien bis an die Horner des Altars;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142792020328450",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "28",
    "text": "Du hist mein Gott, und ich danke dir; mein Gott, ich will dich preisen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138142792552513538",
    "book": "psalms                             ",
    "chapter": "118",
    "verse": "29",
    "text": "Danketdem HERRN; denn eristfreundlich, und seine Gute wahret ewiglich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  }
]