[
  {
    "id": "1138141476394795010",
    "book": "psalms                             ",
    "chapter": "6",
    "verse": "1",
    "text": "Ein Psalm Davids, vorzusingen auf acht Saiten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141476948410370",
    "book": "psalms                             ",
    "chapter": "6",
    "verse": "2",
    "text": "Ach, HERR, strafe mich nicht in deinem Zorn und zuchtige mich nicht in deinem Grimm!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141477478236162",
    "book": "psalms                             ",
    "chapter": "6",
    "verse": "3",
    "text": "HERR, sei mir gnadig, denn ich bin schwach; heile mich, HERR, denn meine Gebeine sind erschrocken,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141478015631362",
    "book": "psalms                             ",
    "chapter": "6",
    "verse": "4",
    "text": "und meine Seele ist sehr erschrocken. Ach, du HERR, wie lange!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141478593691650",
    "book": "psalms                             ",
    "chapter": "6",
    "verse": "5",
    "text": "Wende dich, HERR, und errette meine Seele; hilf mir urn deiner Gute willen!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141479168835586",
    "book": "psalms                             ",
    "chapter": "6",
    "verse": "6",
    "text": "Denn im Tode gedenkt man dein nicht; wer will dir in der Holle danken?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138141479701053442",
    "book": "psalms                             ",
    "chapter": "6",
    "verse": "7",
    "text": "Ich bin so mude vom Seufzen, ich schwemme mein Bette die ganze Nacht und netze mit meinen Tranen mein Lager.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141480228388866",
    "book": "psalms                             ",
    "chapter": "6",
    "verse": "8",
    "text": "Meine Gestalt ist verfallen vor Trauern und ist alt worden; denn ich allenthalben geangstet werde.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141480767455234",
    "book": "psalms                             ",
    "chapter": "6",
    "verse": "9",
    "text": "Weichet von mir, alle Ubeltater; denn der HERR horet mein Weinen,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141481299738626",
    "book": "psalms                             ",
    "chapter": "6",
    "verse": "10",
    "text": "der HERR horet mein Flehen, mein Gebet nimmt der HERR an.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141481873506306",
    "book": "psalms                             ",
    "chapter": "6",
    "verse": "11",
    "text": "Es mussen alle meine Feinde zuschanden werden und sehr erschrecken, sich zuruckkehren und zuschanden werden plotzlich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  }
]