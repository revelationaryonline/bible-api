    "chapter": "57",
    "verse": "12",
    "text": "Erhebe dich, Gott, uber den Himmel und deine Ehre uber alle Welt!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  }
