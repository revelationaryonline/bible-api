[
  {
    "id": "1138142222609448962",
    "book": "psalms                             ",
    "chapter": "85",
    "verse": "1",
    "text": "Ein Psalm der Kinder Korah, vorzusingen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142223201533954",
    "book": "psalms                             ",
    "chapter": "85",
    "verse": "2",
    "text": "HERR, der du bist vormals gnadig gewesen deinem Lande und hast die Gefangenen Jakobs erloset;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138142223778676738",
    "book": "psalms                             ",
    "chapter": "85",
    "verse": "3",
    "text": "der du die Missetat vormals vergeben hast deinem Volk und alle ihre SÃ¼nde bedecket, Sela;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138142224340320258",
    "book": "psalms                             ",
    "chapter": "85",
    "verse": "4",
    "text": "der du vormals hast all deinen Zorn aufgehoben und dich gewendet von dem Grimm deines Zorns;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138142224905863170",
    "book": "psalms                             ",
    "chapter": "85",
    "verse": "5",
    "text": "troste uns, Gott, unser Heiland, und lad ab von deiner Ungnade uber uns!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142225522917378",
    "book": "psalms                             ",
    "chapter": "85",
    "verse": "6",
    "text": "Willst du denn ewiglich uber uns zurnen und deinen Zorn gehen lassen immer fur und fur?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138142226101141506",
    "book": "psalms                             ",
    "chapter": "85",
    "verse": "7",
    "text": "Willst du uns denn nicht wieder erquicken, dad sich dein Volk uber dir freuen moge?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138142226658000898",
    "book": "psalms                             ",
    "chapter": "85",
    "verse": "8",
    "text": "HERR, erzeige uns deine Gnade und hilf uns!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138142227203031042",
    "book": "psalms                             ",
    "chapter": "85",
    "verse": "9",
    "text": "Ach, dad ich horen sollte, das Gott der HERR redet, dad er Frieden zusagte seinem Volk und seinen Heiligen, auf dad sie nicht auf eine Torheit geraten!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138142227770703874",
    "book": "psalms                             ",
    "chapter": "85",
    "verse": "10",
    "text": "Doch istja seine Hilfe nahe denen, die ihn furchten, dad in unserm Lande Ehre wohne;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138142228337098754",
    "book": "psalms                             ",
    "chapter": "85",
    "verse": "11",
    "text": "dad Gute und Treue einander begegnen, Gerechtigkeit und Friede sich kussen;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138142228900970498",
    "book": "psalms                             ",
    "chapter": "85",
    "verse": "12",
    "text": "dad Treue auf der Erde wachse, und Gerechtigkeit vom Himmel schaue;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138142229434105858",
    "book": "psalms                             ",
    "chapter": "85",
    "verse": "13",
    "text": "dad uns auch der HERR Gutes tue, damit unser Land sein Gewachs gebe;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138142229994373122",
    "book": "psalms                             ",
    "chapter": "85",
    "verse": "14",
    "text": "dad Gerechtigkeit dennoch vor ihm bleibe und im Schwange gehe.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  }
]