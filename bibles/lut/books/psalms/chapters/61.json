[
  {
    "id": "1138141962381197314",
    "book": "psalms                             ",
    "chapter": "61",
    "verse": "1",
    "text": "Ein Psalm Davids, vorzusingen auf einem Saitenspiel.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141962927079426",
    "book": "psalms                             ",
    "chapter": "61",
    "verse": "2",
    "text": "Hore, Gott, mein Geschrei und merke auf mein Gebet!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141963514707970",
    "book": "psalms                             ",
    "chapter": "61",
    "verse": "3",
    "text": "Hienieden auf Erden rufe ich zu dir, wenn mein Herz in Angst ist, du wollest mich fuhren auf einen hohen Felsen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138141964099977218",
    "book": "psalms                             ",
    "chapter": "61",
    "verse": "4",
    "text": "Denn du bist meine Zuversicht, ein starker Turm vor meinen Feinden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141964648284162",
    "book": "psalms                             ",
    "chapter": "61",
    "verse": "5",
    "text": "Ich will wohnen in deiner Hütte ewiglich und trauen unter deinen Fittichen. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141965235388418",
    "book": "psalms                             ",
    "chapter": "61",
    "verse": "6",
    "text": "Denn du, Gott, horest meine Gelubde; du belohnest die wohl, die deinen Namen furchten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138141965803716610",
    "book": "psalms                             ",
    "chapter": "61",
    "verse": "7",
    "text": "Du gibsteinem Konige langes Leben, dafi seine Jahre wahren immer fur und fur,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138141966388166658",
    "book": "psalms                             ",
    "chapter": "61",
    "verse": "8",
    "text": "daß er immer sitzen bleibet vor Gott. Erzeige ihm Gute und Treue, die ihn behuten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141966941159426",
    "book": "psalms                             ",
    "chapter": "61",
    "verse": "9",
    "text": "So will ich deinem Namen lobsingen ewiglich, daß ich meine Gelubde bezahle taglich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  }
]