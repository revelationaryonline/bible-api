[
  {
    "id": "1138142339869736962",
    "book": "psalms                             ",
    "chapter": "97",
    "verse": "1",
    "text": "Der HERR ist Konig; des freue sich das Erdreich, und seien frohlich die Inseln, soviel ihrer ist.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142340404805634",
    "book": "psalms                             ",
    "chapter": "97",
    "verse": "2",
    "text": "Wolken und Dunkel ist um ihn her. Gerechtigkeit und Gericht ist seines Stuhles Festung:                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138142340988141570",
    "book": "psalms                             ",
    "chapter": "97",
    "verse": "3",
    "text": "Feuer gehet vor ihm her und zündet an umher seine Feinde.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138142341545754626",
    "book": "psalms                             ",
    "chapter": "97",
    "verse": "4",
    "text": "Seine Blitze leuchten auf den Erdboden; das Erdreich siehet und erschrickt.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138142342110937090",
    "book": "psalms                             ",
    "chapter": "97",
    "verse": "5",
    "text": "Berge zerschmelzen wie Wachs vor dem HERRN, vor dem Herrscher des ganzen Erdbodens.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138142342671925250",
    "book": "psalms                             ",
    "chapter": "97",
    "verse": "6",
    "text": "Die Himmel verkundigen seine Gerechtigkeit, und alle Volker sehen seine Ehre.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138142343228719106",
    "book": "psalms                             ",
    "chapter": "97",
    "verse": "7",
    "text": "Schamen mussen sich alle, die den Bildern dienen und sich der Gotzen ruhmen. Betet ihn an, alle Gotter!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138142343756021762",
    "book": "psalms                             ",
    "chapter": "97",
    "verse": "8",
    "text": "Zion horetes und istfroh; und die Tochter Judas sind frohlich, HERR über deinem Regiment.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138142344297152514",
    "book": "psalms                             ",
    "chapter": "97",
    "verse": "9",
    "text": "Denn du, HERR, bist der Hochste in alien Landen; du bist sehr erhohet über alle Gotter.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142344878522370",
    "book": "psalms                             ",
    "chapter": "97",
    "verse": "10",
    "text": "Die ihrden HERRN liebet, hasset das Arge! Der HERR bewahret die Seelen seiner Heiligen; von der Gottlosen Hand wird er sie erretten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138142345441935362",
    "book": "psalms                             ",
    "chapter": "97",
    "verse": "11",
    "text": "Dem Gerechten muß das Licht immer wieder aufgehen und Freude den frommen Herzen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142345992470530",
    "book": "psalms                             ",
    "chapter": "97",
    "verse": "12",
    "text": "Ihr Gerechten, freuet euch des HERRN; und danket ihm und preiset seine Heiligkeit!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  }
]