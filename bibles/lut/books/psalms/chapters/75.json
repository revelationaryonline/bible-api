[
  {
    "id": "1138142107455193090",
    "book": "psalms                             ",
    "chapter": "75",
    "verse": "1",
    "text": "Ein Psalm und Lied Assaphs, daß er nicht umkame, vorzusingen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142108026077186",
    "book": "psalms                             ",
    "chapter": "75",
    "verse": "2",
    "text": "Wir danken dir, Gott, wir danken dir und verkündigen deine Wunder, daß dein Name so nahe ist.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138142108610625538",
    "book": "psalms                             ",
    "chapter": "75",
    "verse": "3",
    "text": "Denn zu seiner Zeit so werde ich recht richten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138142109158440962",
    "book": "psalms                             ",
    "chapter": "75",
    "verse": "4",
    "text": "Das Land zittert und alle, die drinnen wohnen; aber ich halte seine Saulen test. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142109670309890",
    "book": "psalms                             ",
    "chapter": "75",
    "verse": "5",
    "text": "Ich sprach zu den Ruhmredigen: Rühmet nicht so! und zu den Gottlosen: Pochet nicht auf Gewalt!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138142110224744450",
    "book": "psalms                             ",
    "chapter": "75",
    "verse": "6",
    "text": "Pochet nicht so hoch auf eure Gewalt, redet nicht halsstarrig,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142110809882626",
    "book": "psalms                             ",
    "chapter": "75",
    "verse": "7",
    "text": "es habe keine Not, weder von Aufgang noch von Niedergang, noch von dem Gebirge in der Wuste.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138142111367331842",
    "book": "psalms                             ",
    "chapter": "75",
    "verse": "8",
    "text": "Denn Gott ist Richter, der diesen niedriget und jenen erhohet.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142111936020482",
    "book": "psalms                             ",
    "chapter": "75",
    "verse": "9",
    "text": "Denn der HERR hat einen Becher in der Hand und mit starkem Wein voll eingeschenkt und schenkt aus demselben; aber die Gottlosen müssen alle trinken und die Hefen aussaufen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138142112503201794",
    "book": "psalms                             ",
    "chapter": "75",
    "verse": "10",
    "text": "Ich aber will verkundigen ewiglich und lobsingen dem Gott Jakobs.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142113069727746",
    "book": "psalms                             ",
    "chapter": "75",
    "verse": "11",
    "text": "Und will alle Gewalt der Gottlosen zerbrechen, daß die Gewalt des Gerechten erhohet werde.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  }
]