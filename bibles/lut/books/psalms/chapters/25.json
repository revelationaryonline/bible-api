[
  {
    "id": "1138141629112614914",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "1",
    "text": "Ein Psalm Davids. Nach dir, HERR, verlanget mich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141629657186306",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "2",
    "text": "Mein Gott, ich hoffe auf dich. Laft mich nicht zuschanden werden, daß sich meine Feinde nicht freuen über mich!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141630212866050",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "3",
    "text": "Denn keinerwird zuschanden, der dein harret; aber zuschanden müssen sie werden, die losen Verachter.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138141630766546946",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "4",
    "text": "HERR, zeige mirdeine Wege und lehre mich deine Steige!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138141631308529666",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "5",
    "text": "Leite mich in deiner Wahrheit und lehre mich; denn du bist der Gott, der mir hilft; taglich harre ich dein.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141631864340482",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "6",
    "text": "Gedenke, HERR, an deine Barmherzigkeit und an deine Güte, die von der Welt her gewesen ist.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141632386269186",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "7",
    "text": "Gedenke nicht der Sunden meiner Jugend und meiner Ubertretung; gedenke aber mein nach deiner Barmherzigkeit um deiner Güte willen!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141632944373762",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "8",
    "text": "Der HERR ist gut und fromm, darum unterweiset er die Sunder auf dem Wege.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141633491861506",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "9",
    "text": "Er leitet die Elenden recht und lehret die Elenden seinen Weg.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141634034860034",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "10",
    "text": "Die Wege des HERRN sind eitel Güte und Wahrheit denen, die seinen Bund und Zeugnis halten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141634584477698",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "11",
    "text": "Um deines Namens willen, HERR, sei gnadig meiner Missetat, die da graft ist!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141635144384514",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "12",
    "text": "Wer ist der, der den HERRN fürchtet? Er wird ihn unterweisen den besten Weg.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138141635708289026",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "13",
    "text": "Seine Seele wird im Guten wohnen, und sein Same wird das Land besitzen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138141636218880002",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "14",
    "text": "Das Geheimnis des HERRN ist unter denen, die ihn furchten, und seinen Bund laftt er sie wissen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138141636802215938",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "15",
    "text": "Meine Augen sehen stets zu dem HERRN denn er wird meinen Fuß aus dem Netze ziehen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141637354455042",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "16",
    "text": "Wende dich zu mir und sei mir gnadig; denn ich bin einsam und elend.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141637917835266",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "17",
    "text": "Die Angst meines Herzens ist groß; führe mich aus meinen Noten!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141638460080130",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "18",
    "text": "Siehe an meinen Jammer und Elend und vergib miralle meine Sunde!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138141638997278722",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "19",
    "text": "Siehe, daß meiner Feinde so viel ist und hassen mich aus Frevel.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141639548665858",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "20",
    "text": "Bewahre meine Seele und errette mich; laß mich nicht zuschanden werden, denn ich traue auf dich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141640102445058",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "21",
    "text": "Schlecht und recht, das behute mich; denn ich harre dein.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141640643837954",
    "book": "psalms                             ",
    "chapter": "25",
    "verse": "22",
    "text": "Gott, erlose Israel aus aller, seiner Not!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  }
]