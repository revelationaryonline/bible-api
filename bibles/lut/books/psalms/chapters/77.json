[
  {
    "id": "1138142120827518978",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "1",
    "text": "Ein Psalm Assaphs fur Jeduthun, vorzusingen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138142121378054146",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "2",
    "text": "Ich schreie mit meiner Stimme zu Gott; zu Gott schreie ich, und er erhoret mich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138142121926131714",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "3",
    "text": "In der Zeit meiner Not suche ich den HERRN; meine Hand ist des Nachts ausgereckt und lafit nicht ab; denn meine Seele will sich nicht trosten lassen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138142122478960642",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "4",
    "text": "Wenn ich betrübt bin, so denke ich an Gott; wenn mein Herz in Angsten ist; so rede ich. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142123026513922",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "5",
    "text": "Meine Augen haltstdu, daß sie wachen; ich bin so ohnmachtig, daß ich nicht reden kann.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142123585372162",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "6",
    "text": "Ich denke der alten Zeit, der vorigen Jahre.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138142124137611266",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "7",
    "text": "Ich denke des Nachts an mein Saitenspiel und rede mit meinem Herzen; mein Geist muli forschen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142124703219714",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "8",
    "text": "Wird denn der HERR ewiglich verstolien und keine Gnade mehr erzeigen?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138142125254705154",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "9",
    "text": "Ist's denn ganz und gar aus mit seiner Gute? und hat die Verheitiung ein Ende?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142125822967810",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "10",
    "text": "Hat denn Gott vergessen, gnadig zu sein, und seine Barmherzigkeit vor Zorn verschlossen? Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142126385463298",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "11",
    "text": "Aber doch sprach ich: Ich muli das leiden; die rechte Hand des Hochsten kann alles andern.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138142126918205442",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "12",
    "text": "Darum gedenk ich an die Taten des HERRN; ja, ich gedenke an deine vorigen Wunder                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138142127478800386",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "13",
    "text": "und rede von alien deinen Werken und sage von deinem Tun.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138142128053747714",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "14",
    "text": "Gott dein Weg istheilig. Wo ist so ein machtiger Gott, als du, Gott, hist?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138142128615456770",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "15",
    "text": "Du hist der Gott, der Wunder tut; du hast deine Macht beweiset unter den Volkern.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142129171333122",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "16",
    "text": "Du hast dein Volk erloset gewaltiglich, die Kinder Jakobs und Josephs. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138142129731436546",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "17",
    "text": "Die Wasser sahen dich, Gott; die Wasser sahen dich und angsteten sich, und die Tiefen tobeten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142130297569282",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "18",
    "text": "Die dicken Wolken gossen Wasser; die Wolken donnerten, und die Strahlen führen daher.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142130847318018",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "19",
    "text": "Es donnerte im Himmel; deine Blitze leuchteten auf dem Erdboden; das Erdreich regte sich und bebete davon.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138142131402866690",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "20",
    "text": "Dein Weg war im Meer und dein Pfad in grofien Wassern, und man spurete doch deinen Fufi nicht.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142131955335170",
    "book": "psalms                             ",
    "chapter": "77",
    "verse": "21",
    "text": "Du fuhretest dein Volk wie eine Herde Schafe durch Mose und Aaron.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  }
]