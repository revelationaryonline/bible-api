[
  {
    "id": "1138142974016356354",
    "book": "psalms                             ",
    "chapter": "137",
    "verse": "1",
    "text": "An den Wassern zu Babel saften wir und weineten, wenn wir an Zion gedachten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138142974594023426",
    "book": "psalms                             ",
    "chapter": "137",
    "verse": "2",
    "text": "Unsere FHarfen hingen wir an die Weiden, die drinnen sind.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138142975194300418",
    "book": "psalms                             ",
    "chapter": "137",
    "verse": "3",
    "text": "Denn daselbst hieften uns singen, die uns gefangen hielten, und in unserm Heulen frohlich sein: Lieber, singet uns ein Lied von Zion!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138142975765151746",
    "book": "psalms                             ",
    "chapter": "137",
    "verse": "4",
    "text": "Wie sollten wir des HERRN Lied singen in fremden Landen?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142976336887810",
    "book": "psalms                             ",
    "chapter": "137",
    "verse": "5",
    "text": "Vergesse ich dein, Jerusalem, so werde meiner Rechten vergessen!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138142976901480450",
    "book": "psalms                             ",
    "chapter": "137",
    "verse": "6",
    "text": "Meine Zunge musse an meinem Gaumen kleben, wo ich dein nicht gedenke, wo ich nicht lasse Jerusalem meine hochste Freude sein.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138142977464598530",
    "book": "psalms                             ",
    "chapter": "137",
    "verse": "7",
    "text": "HERR, gedenke der Kinder Edom am Tage Jerusalems, die da sagen: Rein ab, rein ab, bis auf ihren Boden!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142978013626370",
    "book": "psalms                             ",
    "chapter": "137",
    "verse": "8",
    "text": "Du verstorte Tochter Babel, wohl dem, der dir vergelte, wie du uns getan hast!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142978578415618",
    "book": "psalms                             ",
    "chapter": "137",
    "verse": "9",
    "text": "Wohl dem, der deine jungen Kinder nimmt und zerschmettert sie an den Stein!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  }
]