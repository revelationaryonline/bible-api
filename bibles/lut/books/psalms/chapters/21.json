[
  {
    "id": "1138141594721517570",
    "book": "psalms                             ",
    "chapter": "21",
    "verse": "1",
    "text": "Ein Psalm Davids, vorzusingen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141595273428994",
    "book": "psalms                             ",
    "chapter": "21",
    "verse": "2",
    "text": "HERR, der Konig freuet sich in deiner Kraft, und wie sehrfrohlich ist er uber deine Hilfe!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138141595839266818",
    "book": "psalms                             ",
    "chapter": "21",
    "verse": "3",
    "text": "Du gibst ihm seines Herzens Wunsch und weigerst nicht, was sein Mund bittet. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141596389867522",
    "book": "psalms                             ",
    "chapter": "21",
    "verse": "4",
    "text": "Denn du uberschuttest ihn mit gutem Segen; du setzest eine güldene Krone auf sein Haupt.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141596942204930",
    "book": "psalms                             ",
    "chapter": "21",
    "verse": "5",
    "text": "Er bittet dich urns Leben, so gibst du ihm langes Leben immer und ewiglich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141597491953666",
    "book": "psalms                             ",
    "chapter": "21",
    "verse": "6",
    "text": "Er hatgroße Ehre an deiner Hilfe; du legest Lob und Schmuck auf ihn.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138141598039703554",
    "book": "psalms                             ",
    "chapter": "21",
    "verse": "7",
    "text": "Denn du setzest ihn zum Segen ewiglich, du erfreuest ihn mit Freuden deines Antlitzes.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138141598598823938",
    "book": "psalms                             ",
    "chapter": "21",
    "verse": "8",
    "text": "Denn der Konig hoffet auf den HERRN und wird durch die Gute des Hochsten festbleiben.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138141599123144706",
    "book": "psalms                             ",
    "chapter": "21",
    "verse": "9",
    "text": "Deine Hand wird finden alle deine Feinde; deine Rechte wird finden, die dich hassen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141599694618626",
    "book": "psalms                             ",
    "chapter": "21",
    "verse": "10",
    "text": "Du wirstsie machen wie einen Feuerofen, wenn du dreinsehen wirst; der HERR wird sie verschlingen in seinem Zorn; Feuerwird sie fressen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138141600251084802",
    "book": "psalms                             ",
    "chapter": "21",
    "verse": "11",
    "text": "Ihre Frucht wirst du umbringen vom Erdboden und ihren Samen von den Menschenkindern.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141600795131906",
    "book": "psalms                             ",
    "chapter": "21",
    "verse": "12",
    "text": "Denn siegedachten dir Ubelszu tun, und machten Anschlage, die sie; nicht konnten ausfuhren.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141601339441154",
    "book": "psalms                             ",
    "chapter": "21",
    "verse": "13",
    "text": "Denn du wirst sie zur Schulter machen; mit deiner Sehne wirst du gegen ihr Antlitz zielen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138141601903411202",
    "book": "psalms                             ",
    "chapter": "21",
    "verse": "14",
    "text": "HERR, erhebe dich in deiner Kraft, sowollen wirsingen und loben deine Macht.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  }
]