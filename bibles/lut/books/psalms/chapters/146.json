[
  {
    "id": "1138143040731938818",
    "book": "psalms                             ",
    "chapter": "146",
    "verse": "1",
    "text": "Halleluja! Lobe den HERRN, meine Seele!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138143041281130498",
    "book": "psalms                             ",
    "chapter": "146",
    "verse": "2",
    "text": "Ich will den HERRN loben, so lange ich lebe, und meinem Gott lobsingen, weil ich hie bin.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138143041816035330",
    "book": "psalms                             ",
    "chapter": "146",
    "verse": "3",
    "text": "Verlasset euch nicht auf Fursten; sie sind Menschen, die konnen ja nicht helfen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138143042358378498",
    "book": "psalms                             ",
    "chapter": "146",
    "verse": "4",
    "text": "Denn des Menschen Geist muß davon, und er muß wieder zu Erde werden; alsdann sind verloren alle seine Anschlage.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138143042905276418",
    "book": "psalms                             ",
    "chapter": "146",
    "verse": "5",
    "text": "Wohl dem, des Hilfe der Gott Jakobs ist, des Hoffnung auf dem HERRN, seinem Gott, stehet,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138143043460464642",
    "book": "psalms                             ",
    "chapter": "146",
    "verse": "6",
    "text": "der Himmel, Erde, Meer und alles, was drinnen ist, gemacht hat; der Glauben halt ewiglich;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138143044003299330",
    "book": "psalms                             ",
    "chapter": "146",
    "verse": "7",
    "text": "der Recht schaffet denen, so Gewalt leiden; der die Hungrigen speiset. Der HERR loset die Gefangenen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138143044513202178",
    "book": "psalms                             ",
    "chapter": "146",
    "verse": "8",
    "text": "Der HERR macht die Blinden sehend. Der HERR richtet auf, die niedergeschlagen sind. Der HERR liebet die Gerechten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138143045070684162",
    "book": "psalms                             ",
    "chapter": "146",
    "verse": "9",
    "text": "Der HERR behutet Fremdlinge und Waisen und erhalt die Witwen; und kehret zuruck den Weg der Gottlosen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143045591760898",
    "book": "psalms                             ",
    "chapter": "146",
    "verse": "10",
    "text": "Der HERR ist Konig ewiglich, dein Gott, Zion, fur und fur. Halleluja!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  }
]