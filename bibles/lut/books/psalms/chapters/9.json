[
  {
    "id": "1138141497927172098",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "1",
    "text": "Ein Psalm Davids von der schonen Jugend, vorzusingen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138141498464174082",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "2",
    "text": "Ich danke dem HERRN von ganzem Herzen und erzahle alle deine Wunder.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141499022737410",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "3",
    "text": "Ich freue mich und bin frohlich in dir und lobe deinen Namen, du Allerhochster,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138141499572256770",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "4",
    "text": "dafi du meine Feinde hinter sich getrieben hast; sie sind gefallen und umkommen vordir.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138141500145041410",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "5",
    "text": "Denn du fuhrest mein Recht und Sache aus; du sitzest auf dem Stuhl ein rechter Richter.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138141500691349506",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "6",
    "text": "Du schiltst die Heiden und bringest die Gottlosen um; ihren Namen vertilgest du immer und ewiglich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141501258203138",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "7",
    "text": "Die Schwerter des Feindes haben ein Ende; die Stadte hast du umgekehret; ihr Gedachtnis ist umkommen samt ihnen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138141501802610690",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "8",
    "text": "Der HERR aber bleibt ewiglich; er hat seinen Stuhl bereitet zum Gericht.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138141502341709826",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "9",
    "text": "Und er wird den Erdboden recht richten und die Leute regieren rechtschaffen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141502901190658",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "10",
    "text": "Und der HERR ist des Armen Schutz, ein Schutz in der Not.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141503451365378",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "11",
    "text": "Darum hoffen auf dich, die deinen Namen kennen; denn du verlassest nicht, die dich, HERR, suchen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141504016744450",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "12",
    "text": "Lobetden HERRN, der zu Zion wohnet; verkundiget unter den Leuten sein Tun!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138141504573046786",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "13",
    "text": "Denn ergedenkt und fragt nach ihrem Blut; er vergisset nicht des Schreiens der Armen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138141505116045314",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "14",
    "text": "HERR, sei mir gnadig; siehe an mein Elend unter den Feinden, der du mich erhebest aus den Toren des Todes,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138141505668448258",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "15",
    "text": "auf dafi ich erzahle all deinen Preis in den Toren der Tochter Zion, dafi ich frohlich sei uber deine Hilfe.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141506230845442",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "16",
    "text": "Die Heiden sind versunken in der Grube, die sie zugerichtet hatten; ihr Fuli ist gefangen im Netz, das sie gestellet hatten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141506761785346",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "17",
    "text": "So erkennet man, daß der HERR Recht schaffet. Der Gottlose ist verstrickt in dem Werk seiner Hande durchs Wort. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138141507315204098",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "18",
    "text": "Ach, dafi die Gottlosen müfiten zur Holle gekehret werden, alle Heiden, die Gottes vergessen!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141507866394626",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "19",
    "text": "Denn erwird des Armen nicht so ganz vergessen, und die Hoffnung der Elenden wird nicht verloren sein ewiglich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141508421812226",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "20",
    "text": "HERR, stehe auf, daü Menschen nicht Uberhand kriegen; lali alle Heiden vor dir gerichtet werden!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141508975820802",
    "book": "psalms                             ",
    "chapter": "9",
    "verse": "21",
    "text": "Gib ihnen, HERR, einen Meister, dafi die Heiden erkennen dafi sie Menschen sind. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  }
]