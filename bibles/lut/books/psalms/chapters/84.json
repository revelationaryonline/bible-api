[
  {
    "id": "1138142215329775618",
    "book": "psalms                             ",
    "chapter": "84",
    "verse": "1",
    "text": "Ein Psalm der Kinder Korah, auf der Githith vorzusingen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142215896399874",
    "book": "psalms                             ",
    "chapter": "84",
    "verse": "2",
    "text": "Wie lieblich sind deine Wohnungen, HERR Zebaoth!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138142216448442370",
    "book": "psalms                             ",
    "chapter": "84",
    "verse": "3",
    "text": "Meine Seele verlanget und sehnet sich nach den Vorhofen des HERRN; mein Leib und Seele freuen sich in dem lebendigen Gott.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138142217051799554",
    "book": "psalms                             ",
    "chapter": "84",
    "verse": "4",
    "text": "Denn der Vogel hat ein Haus funden und die Schwalbe ihr Nest, da sie Junge hecken, namlich deine Altare, HERR Zebaoth, mein Konig und mein Gott!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138142217603579906",
    "book": "psalms                             ",
    "chapter": "84",
    "verse": "5",
    "text": "Wohl denen, die in deinem Hause wohnen; die loben dich immerdar. Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142218141204482",
    "book": "psalms                             ",
    "chapter": "84",
    "verse": "6",
    "text": "Wohl den Menschen, die dich fur ihre Starke halten und von Herzen dir nach wandeln,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138142218669785090",
    "book": "psalms                             ",
    "chapter": "84",
    "verse": "7",
    "text": "die durch das Jammertal gehen und machen daselbst Brunnen. Und die Lehrer werden mit viel Segen geschmuckt.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138142219221237762",
    "book": "psalms                             ",
    "chapter": "84",
    "verse": "8",
    "text": "Sie erhalten einen Sieg nach dem andern, daß man sehen muli, der rechte Gott sei zu Zion.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138142219797168130",
    "book": "psalms                             ",
    "chapter": "84",
    "verse": "9",
    "text": "HERR, Gott Zebaoth, hore mein Gebet; vernimm es, Gott Jakobs! Sela.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138142220346851330",
    "book": "psalms                             ",
    "chapter": "84",
    "verse": "10",
    "text": "Gott, unser Schild, schaue doch; siehe an das Reich deines Gesalbten!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138142220895223810",
    "book": "psalms                             ",
    "chapter": "84",
    "verse": "11",
    "text": "Denn ein Tag in deinen Vorhofen ist besser denn sonst tausend. Ich will lieber der Tur hüten in meines Gottes Hause, denn lange wohnen in der Gottlosen Hütten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142221487210498",
    "book": "psalms                             ",
    "chapter": "84",
    "verse": "12",
    "text": "Denn Gott der HERR ist Sonne und Schild, der HERRgibt Gnade und Ehre; erwird kein Gutes mangeln lassen den Frommen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138142222077296642",
    "book": "psalms                             ",
    "chapter": "84",
    "verse": "13",
    "text": "HERR Zebaoth, wohl dem Menschen, der sich auf dich verladt!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  }
]