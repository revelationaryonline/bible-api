[
  {
    "id": "1138142599509901314",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "1",
    "text": "Ein Psalm Davids. Lobe den HERRN, meine Seele, und was in mir ist, seinen heiligen Namen!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138142600078491650",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "2",
    "text": "Lobe den HERRN, meine Seele, und vergiß nicht, was er dir Gutes getan hat,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138142600627322882",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "3",
    "text": "der dir alle deine Sünden vergibt und heilet alle deine Gebrechen,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138142601163800578",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "4",
    "text": "der dein Leben vom Verderben erloset, der dich kronet mit Gnade und Barmherzigkeit,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138142601708306434",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "5",
    "text": "der deinen Mund frohlich machet, und du wieder jung wirst wie ein Adler.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142602247766018",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "6",
    "text": "Der HERR schaffet Gerechtigkeit und Gericht alien, die Unrecht leiden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142602823860226",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "7",
    "text": "Er hat seine Wege Mose wissen lassen, die Kinder Israel sein Tun.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142603362533378",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "8",
    "text": "Barmherzig und gnadig ist der HERR, geduldig und von großer Gute.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138142603925487618",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "9",
    "text": "Erwird nicht immerhadern, noch ewiglich Zorn halten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138142604466585602",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "10",
    "text": "Er handelt nicht mit uns nach unsern Sunden und vergilt uns nicht nach unserer Missetat.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142605003948034",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "11",
    "text": "Denn so hoch der Himmel uber der Erde ist, laßt er seine Gnade walten uber die, so ihn furchten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142605557268482",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "12",
    "text": "So feme der Morgen ist vom Abend, lasset er unsere Ubertretung von uns sein.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138142606107803650",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "13",
    "text": "Wie sich ein Vater uber Kinder erbarmet, so erbarmet sich der HERR uber die, so ihn furchten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138142606695235586",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "14",
    "text": "Denn er kennet, was fur ein Gemachte wir sind; er gedenket daran, daß wir Staub sind.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142607259271170",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "15",
    "text": "Ein Mensch ist in seinem Leben wie Gras; er bluhet wie eine Blume auf dem Felde.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138142607798730754",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "16",
    "text": "Wenn der Wind daruber geht, so ist sie nimmer da, und ihre Statte kennet sie nicht mehr.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142608357982210",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "17",
    "text": "Die Gnade aber des HERRN wahretvon Ewigkeit zu Ewigkeit uber die, so ihn furchten, und seine Gerechtigkeit auf Kindeskind                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138142608902455298",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "18",
    "text": "bei denen, die seinen Bund halten und gedenken an seine Gebote, daß sie danach tun.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138142609434083330",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "19",
    "text": "Der HERR hat seinen Stuhl im Himmel bereitet, und sein Reich herrschet uber alles.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138142609998512130",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "20",
    "text": "Lobet den HERRN, ihr seine Engel, ihr starken Helden, die ihr seinen Befehl ausrichtet, daß man hore die Stimme seines Worts.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142610541117442",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "21",
    "text": "Lobet den HERRN, alle seine Heerscharen, seine Diener, die ihr seinen Willen tut!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142611069140994",
    "book": "psalms                             ",
    "chapter": "103",
    "verse": "22",
    "text": "Lobet den HERRN, alle seine Werke, an alien Orten seiner Herrschaft! Lobe den HERRN, meine Seele!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  }
]