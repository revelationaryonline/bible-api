[
  {
    "id": "1138143029210578946",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "1",
    "text": "Ein Lob Davids. Ich will dich erhohen, mein Gott, du Konig, und deinen Namen loben immer und ewiglich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143029753544706",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "2",
    "text": "Ich will dich taglich loben und deinen Namen ruhmen immer und ewiglich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138143030293266434",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "3",
    "text": "Der HERR ist groß und sehr loblich, und seine Große ist unaussprechlich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138143030836494338",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "4",
    "text": "Kindeskinder werden deine Werke preisen und von deiner Gewalt sagen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138143031386177538",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "5",
    "text": "Ich will reden von deiner herrlichen, schonen Pracht und von deinen Wundern,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138143031934222338",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "6",
    "text": "daß man solle reden von deinen herrlichen Taten und daß man erzahle deine Herrlichkeit,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138143032477253634",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "7",
    "text": "daß man preise deine große Güte und deine Gerechtigkeit ruhme.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138143033028706306",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "8",
    "text": "Gnadig und barmherzig ist der HERR, geduldig und von großer Güte.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138143033562103810",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "9",
    "text": "Der HERR ist alien gütig und erbarmet sich aller seiner Werke.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138143034117816322",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "10",
    "text": "Es sollen dir danken, HERR, alle deine Werke, und deine Heiligen dich loben                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138143034665172994",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "11",
    "text": "und die Ehre deines Konigreichs ruhmen und von deiner Gewalt reden,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138143035188772866",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "12",
    "text": "daß den Menschenkindern deine Gewalt kund werde und die ehrliche Pracht deines Konigreichs.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138143035733278722",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "13",
    "text": "Dein Reich ist ein ewiges Reich, und deine Herrschaft wahret fur und fur.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138143036273459202",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "14",
    "text": "Der HERR erhalt alle, die da fallen, und richtet auf alle, die niedergeschlagen sind.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138143036835102722",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "15",
    "text": "Aller Augen warten auf dich, und du gibst ihnen ihre Speise zu seiner Zeit.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138143037389668354",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "16",
    "text": "Du tust deine Hand auf und erfüllest alles, was lebet, mit Wohlgefallen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138143037942628354",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "17",
    "text": "Der HERR ist gerecht in alien seinen Wegen und heilig in alien seinen Werken.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138143038487789570",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "18",
    "text": "Der HERR ist nahe alien, die ihn anrufen, alien, die ihn mit Ernst anrufen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138143039041110018",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "19",
    "text": "Ertut, was die Gottesfu rchtige n begehren, und horet ihrßchreien und hilft ihnen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138143039623495682",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "20",
    "text": "Der HERR behutet alle, die ihn lieben, und wird vertilgen alle Gottlosen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138143040169082882",
    "book": "psalms                             ",
    "chapter": "145",
    "verse": "21",
    "text": "Mein Mund soil des HERRN Lob sagen, und alles Fleisch lobe seinen heiligen Namen immer und ewiglich!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  }
]