[
  {
    "id": "1138142765485293570",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "1",
    "text": "Das ist mir lieb, daß der HERR meine Stimme und mein Flehen horet,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138142766032650242",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "2",
    "text": "daß er sein Ohr zu mir neiget; darum will ich mein Leben lang ihn anrufen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138142766587740162",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "3",
    "text": "Stricke des Todes hatten mich umfangen, und Angst der Holle hatte mich getroffen; ich kam in Jammer und Not.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138142767146106882",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "4",
    "text": "Aber ich rief an den Namen des HERRN: 0 HERR, errette meine Seele!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138142767707095042",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "5",
    "text": "Der HERR ist gnadig und gerecht, und unser Gott ist barmherzig.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138142768264609794",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "6",
    "text": "Der HERR behutet die Einfaltigen. Wenn ich unterliege, so hilft er mir.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138142768802594818",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "7",
    "text": "Sei nun wiederzufrieden, meine Seele; denn der HERR tut dir Gutes.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138142769398939650",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "8",
    "text": "Denn du hast meine Seele aus dem Tode gerissen, mein Auge von den Tranen, meinen Fu(i vom Gleiten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138142769963761666",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "9",
    "text": "Ich will wandeln vor dem HERRN im Lande der Lebendigen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138142770503319554",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "10",
    "text": "Ich glaube, darum rede ich. Ich werde aber sehrgeplagt.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138142771048284162",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "11",
    "text": "Ich sprach in meinem Zagen: Alle Menschen sind Lügner.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138142771607142402",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "12",
    "text": "Wie soil ich dem HERRN vergelten alle seine Wohltat, die er an mir tut?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138142772166426626",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "13",
    "text": "Ich will den heilsamen Kelch nehmen und des HERRN Namen predigen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142772731019266",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "14",
    "text": "Ich will meine Gelubde dem HERRN bezahlen vor all seinem Volk.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142773277589506",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "15",
    "text": "Der Tod seiner Heiligen ist wert gehalten vor dem HERRN.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142773836906498",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "16",
    "text": "0 HERR, ich bin dein Knecht; ich bin dein Knecht, deiner Magd Sohn. Du hast meine Bande zerrissen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138142774375022594",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "17",
    "text": "Dir will ich Dankopfern und des HERRN Namen predigen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138142774926278658",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "18",
    "text": "Ich will meine Gelubde dem HERRN bezahlen vor all seinem Volk,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142775487856642",
    "book": "psalms                             ",
    "chapter": "116",
    "verse": "19",
    "text": "in den Hofen am Hause des HERRN, in dir, Jerusalem. Halleluja!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  }
]