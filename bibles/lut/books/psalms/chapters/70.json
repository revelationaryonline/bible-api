[
  {
    "id": "1138142051038724098",
    "book": "psalms                             ",
    "chapter": "70",
    "verse": "1",
    "text": "Ein Psalm Davids, vorzusingen zum Gedachtnis.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138142051586899970",
    "book": "psalms                             ",
    "chapter": "70",
    "verse": "2",
    "text": "Eile, Gott, mich zu erretten, HERR, mirzu helfen!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142052146446338",
    "book": "psalms                             ",
    "chapter": "70",
    "verse": "3",
    "text": "Es müssen sich schamen und zuschanden werden, die nach meiner Seele stehen; sie müssen zurückkehren und gehohnet werden, die mir übels wünschen,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138142052716904450",
    "book": "psalms                             ",
    "chapter": "70",
    "verse": "4",
    "text": "daß sie müssen wiederum zu Schanden werden, die da über mich schreien: Da, da!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142053286641666",
    "book": "psalms                             ",
    "chapter": "70",
    "verse": "5",
    "text": "Freuen und frohlich müssen sein an dir, die nach dir fragen; und die dein Heil lieben, immersagen: Hochgelobt sei Gott!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142053857230850",
    "book": "psalms                             ",
    "chapter": "70",
    "verse": "6",
    "text": "Ich aber bin elend und arm. Gott, eile zu mir, denn du bist mein Heifer und Erretter; mein Gott, verzeuch nicht!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  }
]