[
  {
    "id": "1138142928416047106",
    "book": "psalms                             ",
    "chapter": "130",
    "verse": "1",
    "text": "Ein Lied im hohern Chor. Aus der Tiefe rufe ich, HERR, zu dir.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142928964124674",
    "book": "psalms                             ",
    "chapter": "130",
    "verse": "2",
    "text": "HERR, hore meine Stimme; lali deine Ohren merken auf die Stimme meines Flehens!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138142929515773954",
    "book": "psalms                             ",
    "chapter": "130",
    "verse": "3",
    "text": "So du willst, HERR, Sünde zurechnen, HERR, wer wird bestehen?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138142930068078594",
    "book": "psalms                             ",
    "chapter": "130",
    "verse": "4",
    "text": "Denn bei dir ist die Vergebung, daß man dich furchte.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142930645647362",
    "book": "psalms                             ",
    "chapter": "130",
    "verse": "5",
    "text": "Ich harre des HERRN; meine Seele harret, und ich hoffe auf sein Wort.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138142931214663682",
    "book": "psalms                             ",
    "chapter": "130",
    "verse": "6",
    "text": "Meine Seele wartet auf den HERRN von einer Morgenwache bis zur andern.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142931769294850",
    "book": "psalms                             ",
    "chapter": "130",
    "verse": "7",
    "text": "Israel hoffe auf den HERRN; denn bei dem HERRN ist die Gnade und viel Erlosung bei ihm;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138142932327890946",
    "book": "psalms                             ",
    "chapter": "130",
    "verse": "8",
    "text": "und erwird Israel erlosen aus alien seinen Sunden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  }
]