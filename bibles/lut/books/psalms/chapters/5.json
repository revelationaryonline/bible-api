[
  {
    "id": "1138141469197402114",
    "book": "psalms                             ",
    "chapter": "5",
    "verse": "1",
    "text": "Ein Psalm Davids, vorzusingen fur das Erbe.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141469742268418",
    "book": "psalms                             ",
    "chapter": "5",
    "verse": "2",
    "text": "HERR, hore meine Worte, merke auf meine Rede!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138141470299914242",
    "book": "psalms                             ",
    "chapter": "5",
    "verse": "3",
    "text": "Vernimm mein Schreien, mein Konig und mein Gott; denn ich will vor dir beten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138141470848319490",
    "book": "psalms                             ",
    "chapter": "5",
    "verse": "4",
    "text": "HERR, frühe wollest du meine Stimme horen; fruhe will ich mich zu dir schicken und drauf merken.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141471426412546",
    "book": "psalms                             ",
    "chapter": "5",
    "verse": "5",
    "text": "Denn du bist nicht ein Gott, dem gottlos Wesen gefallt; wer bose ist, bleibet nicht vor dir.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141471993659394",
    "book": "psalms                             ",
    "chapter": "5",
    "verse": "6",
    "text": "Die Ruhmredigen bestehen nicht vor deinen Augen; du bistfeind alien Ubeltatern.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138141472564903938",
    "book": "psalms                             ",
    "chapter": "5",
    "verse": "7",
    "text": "Du bringest die Lugner urn; der HERR hat Greuel an den Blutgierigen und Falschen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141473126023170",
    "book": "psalms                             ",
    "chapter": "5",
    "verse": "8",
    "text": "Ich aber will in dein Haus gehen auf deine grofte Gute und anbeten gegen deinem heiligen Tempel in deiner Furcht.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141473681702914",
    "book": "psalms                             ",
    "chapter": "5",
    "verse": "9",
    "text": "HERR, leite mich in deiner Gerechtigkeit urn meiner Feinde willen; richte deinen Weg vor mir her.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141474218967042",
    "book": "psalms                             ",
    "chapter": "5",
    "verse": "10",
    "text": "Denn in ihrem Munde ist nichts Gewisses, ihr Inwendiges ist Herzeleid, ihr Rachen ist ein offenes Grab, mit ihren Zungen heucheln sie.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138141474768388098",
    "book": "psalms                             ",
    "chapter": "5",
    "verse": "11",
    "text": "Schuldige sie, Gott, daß sie fallen von ihrem Vornehmen! Stofte sie aus urn ihrergroften Ubertretung willen; denn sie sind dirwiderspenstig.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138141475300704258",
    "book": "psalms                             ",
    "chapter": "5",
    "verse": "12",
    "text": "Laft sich freuen alle, die auf dich trauen; ewiglich laft sie ruhmen, denn du beschirmest sie; frohlich laft sein in dir, die deinen Namen lieben!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141475837542402",
    "book": "psalms                             ",
    "chapter": "5",
    "verse": "13",
    "text": "Denn du, HERR, segnest die Gerechten; du kronest sie mit Gnade wie mit einem Schilde.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  }
]