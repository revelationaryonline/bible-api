[
  {
    "id": "1138142230541926402",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "1",
    "text": "Ein Gebet Davids. HERR, neige deine Ohren und erhore mich; denn ich bin elend und arm.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142231084761090",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "2",
    "text": "Bewahre meine Seele; denn ich bin heilig. Hilf du, mein Gott, deinem Knechte, der sich verladt auf dich!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142231631691778",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "3",
    "text": "HERR, sei mir gnadig; denn ich rufe taglich zu dir.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138142232216338434",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "4",
    "text": "Erfreue die Seele deines Knechts; denn nach dir, HERR, verlanget mich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142232771690498",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "5",
    "text": "Denn du, HERR, bist gut und gnadig, von großer Gute alien, die dich anrufen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138142233330384898",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "6",
    "text": "Vernimm, HERR, mein Gebet und merke auf die Stimme meines Flehens.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138142233873580034",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "7",
    "text": "In der Not rufe ich dich an; du wollest mich erhoren.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138142234433028098",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "8",
    "text": "HERR, es ist dir keiner gleich unter den Gottern und ist niemand, der tun kann wie du.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138142235022032898",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "9",
    "text": "Alle Heiden, die du gemacht hast, werden kommen und vor dir anbeten, HERR, und deinen Namen ehren,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138142235555987458",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "10",
    "text": "daß du so graft bist und Wunder tust und alleine Gott bist.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138142236107112450",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "11",
    "text": "Weise mir, HERR, deinen Weg, daß ich wandele in deiner Wahrheit; erhalte mein Herz bei dem Einigen, daß ich deinen Namen furchte!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138142236670394370",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "12",
    "text": "Ich danke dir, HERR, mein Gott, von ganzem Herzen und ehre deinen Namen ewiglich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138142237223256066",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "13",
    "text": "Denn deine Gute ist graft uber mich, und hast meine Seele errettet aus der tiefen Holle.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142237794500610",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "14",
    "text": "Gott, es setzen sich die Stolzen wider mich, und der Haute der Tyrannen stehet mir nach meiner Seele und haben dich nichtvor Augen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138142238346084354",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "15",
    "text": "Du aber, HERR Gott, bist barmherzig und gnadig, geduldig und von großer Gute und Treue.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138142238889181186",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "16",
    "text": "Wende dich zu mir, sei mir gnadig; starke deinen Knecht mit deiner Macht und hilt dem Sohn deiner Magd!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138142239457738754",
    "book": "psalms                             ",
    "chapter": "86",
    "verse": "17",
    "text": "Tu ein Zeichen an mir, daß mifs wohlgehe, daß es sehen, die mich hassen, und sich schamen müssen, daß du mir beistehest, HERR, und trostest mich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  }
]