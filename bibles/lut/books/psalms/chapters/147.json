[
  {
    "id": "1138143046121553922",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "1",
    "text": "Lobet den HERRN; denn unsern Gott loben, das ist ein kostlich Ding; solch Lob ist lieblich und schon.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138143046664192002",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "2",
    "text": "Der HERR bauet Jerusalem und bringet zusammen die Verjagten in Israel.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143047237992450",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "3",
    "text": "Er heilet, die zerbrochenes Herzens sind, und verbindet ihre Schmerzen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138143047780106242",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "4",
    "text": "Er zahlet die Sterne und nennet sie alle mit Namen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138143048321073154",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "5",
    "text": "Unser HERR ist graft und von grafter Kraft; und ist unbegreiflich, wie er regieret.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138143048896479234",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "6",
    "text": "Der HERR richtet auf die Elenden und stoftet die Gottlosen zu Boden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138143049480208386",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "7",
    "text": "Singet umeinander dem HERRN mit Dank und lobet unsern Gott mit Harfen,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143050030579714",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "8",
    "text": "der den Himmel mit Wolken bedeckt und gibt Regen auf Erden; der Gras auf Bergen wachsen laftt;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138143050575740930",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "9",
    "text": "der dem Vieh sein Futtergibt, den jungen Raben, die ihn anrufen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138143051142332418",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "10",
    "text": "Er hat nicht Lust an der Starke des Rosses noch Gefallen an jemandes Beinen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138143051700502530",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "11",
    "text": "Der HERR hat Gefallen an denen, die ihn furchten, die auf seine GÃ¼te hoffen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138143052249104386",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "12",
    "text": "Praise, Jerusalem, den HERRN; lobe, Zion, deinen Gott!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143052788269058",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "13",
    "text": "Denn er macht fest die Riegel deiner Tore und segnet deine Kinder drinnen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138143053338050562",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "14",
    "text": "Er schaffet deinen Grenzen Frieden und sattiget dich mit dem besten Weizen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138143053933051906",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "15",
    "text": "Er sendet seine Rede auf Erden; sein Wort lauft schnell.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138143054494629890",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "16",
    "text": "Ergibt Schnee wie Wolle, er streuet Reifwie Asche.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138143055073968130",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "17",
    "text": "Er wirft seine Schloften wie Bissen; wer kann bleiben vor seinem Frost?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138143055651176450",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "18",
    "text": "Erspricht, so zerschmilzt es; er laftt seinen Wind wehen, so tauet's auf.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138143056200466434",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "19",
    "text": "Er zeiget Jakob sein Wort, Israel seine Sitten und Rechte.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138143056741236738",
    "book": "psalms                             ",
    "chapter": "147",
    "verse": "20",
    "text": "So tut er keinen Heiden, noch laftt sie wissen seine Rechte. Halleluja!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  }
]