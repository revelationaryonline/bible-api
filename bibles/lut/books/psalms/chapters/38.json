[
  {
    "id": "1138141764089872386",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "1",
    "text": "Ein Psalm Davids zum Gedachtnis.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138141764670324738",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "2",
    "text": "HERR, strafe mich nicht in deinem Zorn und zuchtige mich nicht in deinem Grimm!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138141765227937794",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "3",
    "text": "Denn deine Pfeile stecken in mir, und deine Hand drucket mich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141765778472962",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "4",
    "text": "Es ist nichts Gesundes an meinem Leibe vor deinem Drauen, und ist kein Friede in meinen Gebeinen vor meiner Sunde.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141766317277186",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "5",
    "text": "Denn meine Sünden gehen uber mein Haupt, wie eine schwere Last sind sie mir zu schwer worden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141766880919554",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "6",
    "text": "Meine Wunden stinken und eitern vor meiner Torheit.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141767462060034",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "7",
    "text": "Ich gehe krumm und sehr gebuckt; den ganzen Tag gehe ich traurig.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141768025866242",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "8",
    "text": "Denn meine Lenden verdorren ganz, und ist nichts Gesundes an meinem Leibe.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138141768594325506",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "9",
    "text": "Es ist mit mir gar anders und bin sehr zerstoden. Ich heule vor Unruhe meines Herzens.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138141769138012162",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "10",
    "text": "HERR, vor dir ist alle meine Begierde und mein Seufzen ist dir nicht verborgen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138141769690284034",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "11",
    "text": "Mein Herz bebet, meine Kraft hat mich verlassen, und das Licht meiner Augen ist nicht bei mir.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141770231808002",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "12",
    "text": "Meine Lieben und Freunde stehen gegen mich und scheuen meine Plage, und meine Nachsten treten feme.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141770777788418",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "13",
    "text": "Und die mir nach der Seele stehen, stellen mir; und die mir ubel wollen, reden, wie sie Schaden tun wollen, und gehen mit eitel Listen urn.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141771346608130",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "14",
    "text": "Ich aber mud sein wie ein Tauber und nicht horen, und wie ein Stummer, derseinen Mund nicht auftut.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141771917950978",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "15",
    "text": "Und mud sein wie einer, der nicht horet und der keine Widerrede in seinem Munde hat.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141772494143490",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "16",
    "text": "Aber ich harre, HERR, auf dich; du, HERR, mein Gott, wirst erhoren.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141773058441218",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "17",
    "text": "Denn ich denke, daß sieja sich nicht uber mich freuen. Wenn mein Fufi wankete; würden sie sich hoch rühmen wider mich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141773613072386",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "18",
    "text": "Denn ich bin zu Leiden gemacht, und mein Schmerz ist immer vor mir.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141774180679682",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "19",
    "text": "Denn ich zeige meine Missetat an und sorge fur meine Sunde.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141774731640834",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "20",
    "text": "Aber meine Feinde leben und sind machtig; die mich unbillig hassen, sind grod.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141775268478978",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "21",
    "text": "Und die mir Arges tun urn Gutes, setzen sich wider mich, darum dad ich ob dem Guten halte.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138141775850733570",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "22",
    "text": "Verlafi mich nicht, HERR, mein Gott; sei nicht feme von mir!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141776409133058",
    "book": "psalms                             ",
    "chapter": "38",
    "verse": "23",
    "text": "Eile mir beizustehen, HERR, meine Hilfe!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  }
]