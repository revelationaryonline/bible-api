[
  {
    "id": "1138122358358900738",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "1",
    "text": "Und der HERR redete mit Mose und Aaron und sprach:                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138122358925164546",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "2",
    "text": "Die Kinder Israel sollen vor der Hutte des Stiffs umher sich lagern, ein jeglicher unter seinem Panier und Zeichen nach ihrer Vater Hause.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138122359456595970",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "3",
    "text": "Gegen Morgen soil sich lagern Juda mit seinem Panier und Heer; ihr Hauptmann Nahesson, der Sohn Amminadabs;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138122360012079106",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "4",
    "text": "und sein Heer an der Summa vierundsiebenzigtausend und sechshundert.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138122360543346690",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "5",
    "text": "Neben ihm soil sich lagern der Stamm Isaschar; ihr Hauptmann Nethaneel, der Sohn Zuars;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138122361111478274",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "6",
    "text": "und sein Heer an der Summa vierundfunfzigtausend und vierhundert.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138122361634029570",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "7",
    "text": "Dazu der Stamm Sebulon; ihr Hauptmann Eliab, der Sohn Helons;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138122362156023810",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "8",
    "text": "sein Heer an der Summa siebenundfünfzigtausend und vierhundert.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138122362665238530",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "9",
    "text": "Dali alle, die ins Lager Judas gehoren, seien an der Summa hundertundsechsundachtzigtausend und vierhundert, die zu ihrem Heere gehoren; und sollen vornean ziehen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138122363176222722",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "10",
    "text": "Gegen Mittag soli liegen das Gezelt und Panier Rubens mit ihrem Heer; ihr Hauptmann Elizur, der Sohn Sedeurs;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138122363711291394",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "11",
    "text": "und sein Heer an der Summa sechsundvierzigtausend funfhundert.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138122364243705858",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "12",
    "text": "Neben ihm soli sich lagern der Stamm Simeon; ihr Hauptmann Selumiel, der Sohn Zuri-Saddais;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138122364783493122",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "13",
    "text": "und sein Heer an der Summa neunundfunfzigtausend dreihundert.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138122365314727938",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "14",
    "text": "Dazu der Stamm Gad; ihr Hauptmann Eliasaph, der Sohn Reguels;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138122365831675906",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "15",
    "text": "und sein Heer an der Summa funfundvierzigtausend sechshundertundfunfzig.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138122366388338690",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "16",
    "text": "Dali alle, die ins Lager Rubens gehoren, seien an der Summa hundertundeinundfünfzigtausend vierhundertundfünfzig, die zu ihrem Heer gehoren; und sollen die andern im Ausziehen sein.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138122366938415106",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "17",
    "text": "Danach soli die Hutte des Stitts ziehen mit dem Lager der Leviten mitten unter den Lagern; und wie sie sich lagern, so sollen sie auch ziehen, ein jeglicher an seinem Ort; unter seinem Panier.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138122367462309890",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "18",
    "text": "Gegen Abend soil liegen das Gezelt und Panier Ephraims mit ihrem Heer; ihr Hauptmann soil sein Elisama, der Sohn Ammihuds;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138122367994494978",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "19",
    "text": "und sein Heer an der Summa vierzigtausend und funfhundert.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138122368589758466",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "20",
    "text": "Neben ihm soli sich lagern der Stamm Manasse; ihr Hauptmann Gamliel, der Sohn Pedazurs;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138122369123024898",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "21",
    "text": "sein Heer an der Summa zweiunddreilügtausend und zweihundert.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138122369656782850",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "22",
    "text": "Dazu der Stamm Benjamin; ihr Hauptmann Abidan, der Sohn Gideonis;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138122370201714690",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "23",
    "text": "sein Heer an der Summa funfunddreißigtausend und vierhundert.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138122370765193218",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "24",
    "text": "Dali alle, die ins Lager Ephraims gehoren, seien an der Summa hundertundachttausend und einhundert, die zu seinem Heer gehoren; und sollen die dritten im Ausziehen sein.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138122371283124226",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "25",
    "text": "Gegen Mitternacht soil liegen das Gezelt und Panier Dans mit ihrem Heer; ihr Hauptmann Ahieser der Sohn Ammi-Saddais;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138122371826581506",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "26",
    "text": "sein Heer an der Summa zweiundsechzigtausend und siebenhundert.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138122372380065794",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "27",
    "text": "Neben ihm soli sich lagern der Stamm Asser; ihr Hauptmann Pagel, der Sohn Ochrans;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138122372911726594",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "28",
    "text": "sein Heer an der Summa einundvierzigtausend und funfhundert.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138122373445058562",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "29",
    "text": "Dazu der Stamm Naphthali; ihr Hauptmann Ahira, der Sohn Enans;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138122373990023170",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "30",
    "text": "sein Heer an der Summa dreiundfunfzigtausend und vierhundert.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138122374514114562",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "31",
    "text": "Dali alle, die ins Lager Dan gehoren, seien an der Summa hundertundsiebenundfunfzigtausend und sechshundert; und sollen die letzten sein im Ausziehen mit ihrem Panier.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138122375068418050",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "32",
    "text": "Das ist die Summa der Kinder Israel nach ihrer Vater Hausern und Lagern mit ihren Heeren: sechshunderttausend und dreitausend funfhundertundfunzig.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138122375610695682",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "33",
    "text": "Aber die Leviten wurden nicht in die Summa unter die Kinder Israel gezahlet, wie der HERR Mose geboten hatte.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138122376181841922",
    "book": "numbers                            ",
    "chapter": "2",
    "verse": "34",
    "text": "Und die Kinder Israel taten alles, wie der HERR Mose geboten hatte, und lagerten sich unter ihre Paniere und zogen aus, ein jeglicher in seinem Geschlecht nach ihrer Vater Hause.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  }
]