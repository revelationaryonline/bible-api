  },
  {
    "id": "1138122897737613314",
    "book": "numbers                            ",
    "chapter": "29",
    "verse": "38",
    "text": "dazu einen Bock zum Sündopfer uber das tagliche Brandopfer mit seinem Speisopfer und seinem Trankopfer.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138122898254987266",
    "book": "numbers                            ",
    "chapter": "29",
    "verse": "39",
    "text": "Solches sollt ihrdem HERRN tun auf eure Feste, ausgenommen, was ihr gelobet und freiwillig gebet zu Brandopfern, Speisopfern, Trankopfern und Dankopfern.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  }
