[
  {
    "id": "1138119980386385922",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "1",
    "text": "Und der HERR suchte heim Sara wie er geredet hatte, und tat mit ihr, wie er geredet hatte.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138119980929482754",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "2",
    "text": "Und Sara ward schwanger und gebar Abraham einen Sohn in seinem Alter um die Zeit, die ihm Gott geredet hatte.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138119981457702914",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "3",
    "text": "Und Abraham hieli seinen Sohn, der ihm geboren war, Isaak, den ihm Sara gebar,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138119981996179458",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "4",
    "text": "und beschnitt ihn am achten Tage, wie ihm Gott geboten hatte.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138119982514307074",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "5",
    "text": "Hundert Jahre war Abraham alt, da ihm sein Sohn Isaak geboren ward.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138119983028568066",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "6",
    "text": "Und Sara sprach: Gott hat mir ein lachen zugerichtet; denn wer es horen wird, der wird mein lachen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138119983561506818",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "7",
    "text": "Und sprach: Wer dürfte von Abraham sagen, daß Sara Kinder sauget und hatte ihm einen Sohn geboren in seinem Alter?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138119984087498754",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "8",
    "text": "Und das Kind wuchs und ward entwohnet. Und Abraham machte ein groli Mahl am Tage, da Isaak entwohnet ward.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138119984655138818",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "9",
    "text": "Und Sara sah den Sohn Hagars, der Agyptischen, den sie Abraham geboren hatte, daß er ein Spotter war,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138119985196957698",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "10",
    "text": "und sprach zu Abraham: Treibe diese Magd aus mit ihrem Sohn; denn dieser Magd Sohn soil nicht erben mit meinem Sohn Isaak.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138119985742315522",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "11",
    "text": "Das Wort gefiel Abraham sehr libel um seines Sohnes willen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138119986264014850",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "12",
    "text": "Aber Gott sprach zu ihm: Lali dir's nicht ubel gefallen des Knaben und der Magd halben. Alles, was Sara dir gesagt hat, dem gehorche. Denn in Isaak soil dir der Same genannt werden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138119986811371522",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "13",
    "text": "Auch will ich der Magd Sohn zum Volk machen, darum daß er deines Samens ist.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138119987387760642",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "14",
    "text": "Da stund Abraham des Morgens fruhe auf und nahm Brot und eine Flasche mit Wasser und legte es Hagar auf ihre Schulter, und den Knaben mit, und liefi sie aus. Da zog sie hin und ging in der Wuste irre bei Bersaba.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138119987917324290",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "15",
    "text": "Da nun das Wasser in der Flasche aus war, warf sie den Knaben unter einen Baum                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138119988480802818",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "16",
    "text": "und ging hin und setzte sich gegenübervon feme, eines Bogenschusses weit; denn sie sprach: Ich kann nichtzusehen des Knaben Sterben. Und sie setzte sich gegenüber und hub ihre Stimme auf und weinete.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138119989018034178",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "17",
    "text": "Da erhorete Gott die Stimme des Knaben. Und der Engel Gottes rief vom Himmel der Hagar und sprach zu ihr: Was ist dir, Hagar? Furchte dich nicht; denn Gott hat erhoret die Stimme des Knaben, da er liegt.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138119989561950210",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "18",
    "text": "Stehe auf, nimm den Knaben und fuhre ihn an deiner Hand; denn ich will ihn zum grolien Volk machen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138119990079782914",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "19",
    "text": "Und Gott tat ihr die Augen auf, daß sie einen Wasserbrunnen sah. Da ging sie hin und fullete die Flasche mit Wasser und trankte den Knaben.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138119990628155394",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "20",
    "text": "Und Gott war mit dem Knaben; der wuchs und wohnete in der Wuste und ward ein guter Schutze;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138119991159848962",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "21",
    "text": "und wohnete in der Wuste Pharan. Und seine Mutter nahm ihm ein Weib aus Agyptenland.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138119991691378690",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "22",
    "text": "Zu derselbigen Zeit redete Abimelech und Phichol, sein Feldhauptmann, mit Abraham und sprach: Gott ist mit dir in allem, das du tust.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138119992235491330",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "23",
    "text": "So schwore mir nun bei Gott, daß du mir, noch meinen Kindern, noch meinen Neffen keine Untreue erzeigen wollest, sondern die Barmherzigkeit, die ich an dir getan habe, an mir auch tust und an dem Lande, da du ein Fremdling innen bist.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138119992737628162",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "24",
    "text": "Da sprach Abraham: Ich will schworen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138119993271058434",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "25",
    "text": "Und Abraham strafte Abimelech urn des Wasserbrunnens willen, den Abimelechs Knechte hatten mit Gewalt genommen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138119993795641346",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "26",
    "text": "Da antwortete Abimelech: Ich hab's nicht gewuftt, wer das getan hat; auch hast du mir's nicht angesagt; dazu habe ich's nicht gehoret denn heute.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138119994337492994",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "27",
    "text": "Da nahm Abraham Schafe und Rinder und gab sie Abimelech; und machten beide einen Bund miteinander.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138119994916732930",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "28",
    "text": "Und Abraham stellete dar sieben Lammer besonders.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138119995476934658",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "29",
    "text": "Da sprach Abimelech zu Abraham: Was sollen die sieben Lammer, die du besonders dargestellt hast?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138119996006891522",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "30",
    "text": "Er antwortete: Sieben Lammer sollst du von meiner Hand nehmen, daß sie mir zum Zeugnis seien, daß ich diesen Brunnen gegraben habe.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138119996550053890",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "31",
    "text": "Daher heiftt die Statte Bersaba, daß sie beide miteinander da geschworen haben.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138119997079322626",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "32",
    "text": "Und also machten sie den Bund zu Bersaba. Da machten sich auf Abimelech und Phichol, sein Feldhauptmann, und zogen wieder in der Philister Land.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138119997639393282",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "33",
    "text": "Abraham aber pflanzete Baume zu Bersaba und predigte daselbst von dem Namen des HERRN, des ewigen Gottes.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138119998179508226",
    "book": "genesis                            ",
    "chapter": "21",
    "verse": "34",
    "text": "Und war ein Fremdling in der Philister Lande eine lange Zeit.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  }
]