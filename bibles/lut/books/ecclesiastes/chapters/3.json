[
  {
    "id": "1138143956241055746",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "1",
    "text": "Ein jegliches hat seine Zeit, und alles Vornehmen unter dem Himmel hat seine Stunde.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138143956829372418",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "2",
    "text": "Geboren werden, Sterben, Pflanzen, Ausrotten, das gepflanzt ist,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138143957358673922",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "3",
    "text": "Würgen, Heilen, Brechen, Bauen,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138143957911568386",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "4",
    "text": "Weinen, Lachen, Klagen, Tanzen,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138143958480879618",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "5",
    "text": "Steine zerstreuen, Steine sammeln, Herzen, Fernen von Herzen,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138143959067197442",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "6",
    "text": "Suchen, Verlieren, Behalten, Wegwerfen,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138143959637786626",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "7",
    "text": "Zerreifien, Zunahen, Schweigen, Reden,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143960199593986",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "8",
    "text": "Lieben, Hassen, Streit, Friede hat seine Zeit.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138143960760418306",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "9",
    "text": "Man arbeite, wie man will, so kann man nicht mehr ausrichten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138143961326682114",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "10",
    "text": "Dahersah ich die Muhe, die Gott den Menschen gegeben hat, daß sie drinnen geplagt werden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138143961867026434",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "11",
    "text": "Er aber tut alles fein zu seiner Zeit und lalit ihr Herz sich angsten, wie es gehen solle in der Welt; denn der Mensch kann doch nicht treffen das Werk, das Gott tut, weder Anfang noch Ende.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138143962450493442",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "12",
    "text": "Darum merkte ich, daß nichts Besseres drinnen ist, denn frdhlich sein und ihm gutlich tun in seinem Leben.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138143963011088386",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "13",
    "text": "Denn eine jeglicher Mensch, der da isset und trinkt und hat guten Mut in all seiner Arbeit, das ist eine Gabe Gottes.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138143963532165122",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "14",
    "text": "Ich merkte, daß alles, was Gott tut, das bestehet immer; man kann nichts dazutun noch abtun; und solches tut Gott, daß man sich vor ihm furchten soil.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138143964075425794",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "15",
    "text": "Was Gott tut, das stehet da; und was er tun will, das muli werden; denn er trachtet und jagt ihm nach.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143964630220802",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "16",
    "text": "Weitersah ich unter der Sonne Stattedes Gerichts, da war ein gottlos Wesen, und Statte der Gerechtigkeit, da waren Gottlose.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138143965191831554",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "17",
    "text": "Da dachte ich in meinem Herzen: Gott muli richten den Gerechten und Gottlosen; denn es hat ailes Vornehmen seine Zeit und alle Werke.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138143965749444610",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "18",
    "text": "Ich sprach in meinem Herzen von dem Wesen der Menschen, darin Gott anzeigt und lalit es ansehen, als waren sie unter sich selbst wie das Vieh.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138143966293000194",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "19",
    "text": "Denn es gehet dem Menschen wie dem Vieh: wie dies stirbt, so stirbt er auch, und haben alle einerlei Odem; und der Mensch hat nichts mehr denn das Vieh; denn es ist alles eitel.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138143966855102466",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "20",
    "text": "Es fahret alles an einen Ort; es ist alles von Staub gemacht und wird wieder zu Staub.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138143967392858114",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "21",
    "text": "Wer weili, ob der Odem der Menschen aufwarts fahre und der Odem des Viehes unterwarts unter die Erde fahre?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138143967964954626",
    "book": "ecclesiastes                       ",
    "chapter": "3",
    "verse": "22",
    "text": "Darum sah ich, daß nichts Besseres ist, denn daß ein Mensch frohlich sei in seiner Arbeit; denn das ist sein Teil. Denn wer will ihn dahin bringen, daß er sehe was nach ihm geschehen wird?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  }
]