[
  {
    "id": "1138141085050863618",
    "book": "job                                ",
    "chapter": "29",
    "verse": "1",
    "text": "Und Hiob hub abermal an seine Spruche und sprach:                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141085582655490",
    "book": "job                                ",
    "chapter": "29",
    "verse": "2",
    "text": "O daß ich ware wie in den vorigen Monden, in den Tagen, da mich Gott behutete,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138141086125981698",
    "book": "job                                ",
    "chapter": "29",
    "verse": "3",
    "text": "da seine Leuchte uber meinem Haupte schien, und ich bei seinem Licht in der Finsternis ging;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141086659739650",
    "book": "job                                ",
    "chapter": "29",
    "verse": "4",
    "text": "wie ich war zur Zeit meiner Jugend, da Gottes Geheimnis uber meiner Hutte war;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141087223676930",
    "book": "job                                ",
    "chapter": "29",
    "verse": "5",
    "text": "da der Allmachtige noch mit mir war und meine Kinder um mich her;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141087758319618",
    "book": "job                                ",
    "chapter": "29",
    "verse": "6",
    "text": "da ich meine Tritte wusch in Butter, und die Felsen mir Olbache gossen;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138141088297713666",
    "book": "job                                ",
    "chapter": "29",
    "verse": "7",
    "text": "da ich ausging zum Tor in der Stadt und lieft meinen Stuhl auf der Gasse bereiten;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141088845889538",
    "book": "job                                ",
    "chapter": "29",
    "verse": "8",
    "text": "da mich die Jungen sahen und sich versteckten, und die Alten vor mir aufstunden;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138141089400684546",
    "book": "job                                ",
    "chapter": "29",
    "verse": "9",
    "text": "da die Obersten aufhoreten zu reden, und legten ihre Hand auf ihren Mund;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141089937620994",
    "book": "job                                ",
    "chapter": "29",
    "verse": "10",
    "text": "da die Stimme der Fursten sich verkroch, und ihre Zunge an ihrem Gaumen klebte.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138141090522890242",
    "book": "job                                ",
    "chapter": "29",
    "verse": "11",
    "text": "Denn welches Ohr mich horete, der preisete mich selig, und welches Auge mich sah, der ruhmte mich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141091064676354",
    "book": "job                                ",
    "chapter": "29",
    "verse": "12",
    "text": "Denn ich errettete den Armen, der da schrie, und den Waisen, der keinen Heifer hatte.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138141091668131842",
    "book": "job                                ",
    "chapter": "29",
    "verse": "13",
    "text": "Der Segen des, der verderben sollte, kam uber mich; und ich erfreuete das Herz der Witwe.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141092213063682",
    "book": "job                                ",
    "chapter": "29",
    "verse": "14",
    "text": "Gerechtigkeit war mein Kleid, das ich anzog wie einen Rock; und mein Rechtwar mein fürstlicher Hut.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141092765106178",
    "book": "job                                ",
    "chapter": "29",
    "verse": "15",
    "text": "Ich war des Blinden Auge und des Lahmen Fufie.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141093349588994",
    "book": "job                                ",
    "chapter": "29",
    "verse": "16",
    "text": "Ich war ein Vater der Armen; und welche Sache ich nicht wuftte, die erforschete ich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141093900877826",
    "book": "job                                ",
    "chapter": "29",
    "verse": "17",
    "text": "Ich zerbrach die Backenzahne des Ungerechten und rift den Raub aus seinen Zahnen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141094455771138",
    "book": "job                                ",
    "chapter": "29",
    "verse": "18",
    "text": "Ich gedachte: Ich will in meinem Nest ersterben und meiner Tage viel machen wie Sand.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138141095005224962",
    "book": "job                                ",
    "chapter": "29",
    "verse": "19",
    "text": "Meine Saat ging auf am Wasser; und der Tau blieb uber meiner Ernte.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141095577092098",
    "book": "job                                ",
    "chapter": "29",
    "verse": "20",
    "text": "Meine Herrlichkeit erneuerte sich immer an mir; und mein Bogen besserte sich in meiner Hand.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141096136474626",
    "book": "job                                ",
    "chapter": "29",
    "verse": "21",
    "text": "Man horete mirzu, und schwiegen und warteten auf meinen Rat.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141096689369090",
    "book": "job                                ",
    "chapter": "29",
    "verse": "22",
    "text": "Nach meinen Worten redete niemand mehr; und meine Rede troff auf sie.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138141097225420802",
    "book": "job                                ",
    "chapter": "29",
    "verse": "23",
    "text": "Sie warteten auf mich wie auf den Regen und sperreten ihren Mund auf als nach dem Abendregen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138141097776480258",
    "book": "job                                ",
    "chapter": "29",
    "verse": "24",
    "text": "Wenn ich sie anlachte, wurden sie nicht zu kühn darauf, und das Licht meines Angesichts machte mich nicht geringer.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141098307977218",
    "book": "job                                ",
    "chapter": "29",
    "verse": "25",
    "text": "Wenn ich zu ihrem Geschafte wollte kommen, so muftte ich obenan sitzen und wohnete wie ein Konig unter Kriegsknechten, da ich trostete, die Leid trugen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  }
]