[
  {
    "id": "1138140955037499394",
    "book": "job                                ",
    "chapter": "19",
    "verse": "1",
    "text": "Hiob antwortete und sprach:                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138140955595800578",
    "book": "job                                ",
    "chapter": "19",
    "verse": "2",
    "text": "Was plaget ihrdoch meine Seele und peiniget mich mit Worten?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138140956138012674",
    "book": "job                                ",
    "chapter": "19",
    "verse": "3",
    "text": "Ihr habt mich nun zehnmal gehohnet und schamet euch nicht, daß ihr mich also umtreibet.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138140956710928386",
    "book": "job                                ",
    "chapter": "19",
    "verse": "4",
    "text": "Irre ich, so irre ich mir.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138140957252845570",
    "book": "job                                ",
    "chapter": "19",
    "verse": "5",
    "text": "Aber ihr erhebet euch wahrlich wider mich und scheltet mich zu meiner Schmach.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138140957809016834",
    "book": "job                                ",
    "chapter": "19",
    "verse": "6",
    "text": "Merket doch einst, daß mir Gott unrecht tut und hat mich mit seinem Jagestrick umgeben.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138140958352080898",
    "book": "job                                ",
    "chapter": "19",
    "verse": "7",
    "text": "Siehe, ob ich schon schreie uber Frevel, so werde ich doch nicht erhoret; ich rufe, und ist kein Recht da.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138140958897405954",
    "book": "job                                ",
    "chapter": "19",
    "verse": "8",
    "text": "Er hat meinen Weg verzaunet, daß ich nicht kann hinubergehen, und hat Finsternis auf meinen Steig gestellet.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138140959446466562",
    "book": "job                                ",
    "chapter": "19",
    "verse": "9",
    "text": "Er hat meine Ehre mir ausgezogen und die Krone von meinem Haupt genommen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138140960001753090",
    "book": "job                                ",
    "chapter": "19",
    "verse": "10",
    "text": "Er hat mich zerbrochen urn und urn und laßt mich gehen, und hat ausgerissen meine Hoffnung wie einen Baum.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138140960565952514",
    "book": "job                                ",
    "chapter": "19",
    "verse": "11",
    "text": "Sein Zorn ist über mich ergrimmet, und er achtet mich fur seinen Feind.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138140961096630274",
    "book": "job                                ",
    "chapter": "19",
    "verse": "12",
    "text": "Seine Kriegsleute sind miteinander kommen und haben ihren Weg über mich gepflastert und haben sich urn meine Hütte her gelagert.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138140961672003586",
    "book": "job                                ",
    "chapter": "19",
    "verse": "13",
    "text": "Er hat meine Brüder feme von mir getan, und meine Verwandten sind mirfremd worden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138140962240299010",
    "book": "job                                ",
    "chapter": "19",
    "verse": "14",
    "text": "Meine Nachsten haben sich entzogen, und meine Freunde haben mein vergessen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138140962802237442",
    "book": "job                                ",
    "chapter": "19",
    "verse": "15",
    "text": "Meine Hausgenossen und meine Magde achten mich fürfremd, ich bin unbekannt worden vor ihren Augen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138140963414147074",
    "book": "job                                ",
    "chapter": "19",
    "verse": "16",
    "text": "Ich rief meinem Knecht, und er antwortete mir nicht; ich mußte ihm flehen mit eigenem Munde.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138140963965698050",
    "book": "job                                ",
    "chapter": "19",
    "verse": "17",
    "text": "Mein Weib stellet sich fremd, wenn ich ihr rufe; ich muß flehen den Kindern meines Leibes.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138140964505944066",
    "book": "job                                ",
    "chapter": "19",
    "verse": "18",
    "text": "Auch die jungen Kinder geben nichts auf mich; wenn ich mich wider sie setze, so geben sie mir bose Worte.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138140965062508546",
    "book": "job                                ",
    "chapter": "19",
    "verse": "19",
    "text": "Alle meine Getreuen haben Greuel an mir; und die ich liebhatte, haben sich wider mich gekehret.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138140965611732994",
    "book": "job                                ",
    "chapter": "19",
    "verse": "20",
    "text": "Mein Gebein hanget an meiner Haut und Fleisch, und kann meine Zahne mit der Haut nicht bedecken.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138140966186942466",
    "book": "job                                ",
    "chapter": "19",
    "verse": "21",
    "text": "Erbarmet euch mein, erbarmet euch mein, ihr, meine Freunde; denn die Hand Gottes hat mich geruhret.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138140966731546626",
    "book": "job                                ",
    "chapter": "19",
    "verse": "22",
    "text": "Warum verfolget ihr mich gleich so wohl als Gott und konnet meines Fleisches nicht satt werden?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138140967251017730",
    "book": "job                                ",
    "chapter": "19",
    "verse": "23",
    "text": "Ach, daß meine Reden geschrieben wurden! Ach, daß sie in ein Buch gestellet wurden,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138140967798276098",
    "book": "job                                ",
    "chapter": "19",
    "verse": "24",
    "text": "mit einem eisernen Griffel auf Blei und zu ewigem Gedachtnis in einen Fels gehauen wurden!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138140968333869058",
    "book": "job                                ",
    "chapter": "19",
    "verse": "25",
    "text": "Aber ich weiß, daß mein Erloser lebet; und er wird mich hernach aus der Erde auferwecken;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138140968918679554",
    "book": "job                                ",
    "chapter": "19",
    "verse": "26",
    "text": "und werde danach mit dieser meiner Haut umgeben werden und werde in meinem Fleisch Gott sehen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138140969431302146",
    "book": "job                                ",
    "chapter": "19",
    "verse": "27",
    "text": "Denselben werde ich mir sehen, und meine Augen werden ihn schauen, und kein Fremder. Meine Nieren sind verzehret in meinem Schoß.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138140969977839618",
    "book": "job                                ",
    "chapter": "19",
    "verse": "28",
    "text": "Denn ihr sprechet: Wie wollen wir ihn verfolgen und eine Sache zu ihm finden?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138140970519920642",
    "book": "job                                ",
    "chapter": "19",
    "verse": "29",
    "text": "Furchtet euch vor dem Schwert; denn das Schwert ist der Zorn uber die Missetat, auf daß ihr wisset, daß ein Gericht sei.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  }
]