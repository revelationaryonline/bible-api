[
  {
    "id": "1138141198383480834",
    "book": "job                                ",
    "chapter": "36",
    "verse": "1",
    "text": "Elihu redete weiter und sprach:                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138141198963048450",
    "book": "job                                ",
    "chapter": "36",
    "verse": "2",
    "text": "Harre mir noch ein wenig, ich will dir's zeigen; denn ich habe noch von Gottes wegen was zu sagen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141199499952130",
    "book": "job                                ",
    "chapter": "36",
    "verse": "3",
    "text": "Ich will meinen Verstand weit holen und meinen Schopfer beweisen, daß er recht sei.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141200035250178",
    "book": "job                                ",
    "chapter": "36",
    "verse": "4",
    "text": "Meine Reden sollen ohne Zweifel nicht falsch sein, mein Verstand soil ohne Wandel vor dir sein.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138141200582967298",
    "book": "job                                ",
    "chapter": "36",
    "verse": "5",
    "text": "Siehe, Gott verwirft die Machtigen nicht; denn er ist auch machtig von Kraft des Herzens.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141201131831298",
    "book": "job                                ",
    "chapter": "36",
    "verse": "6",
    "text": "Den Gottlosen erhalt er nicht, sondern hilft dem Elenden zum Rechten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138141201681383426",
    "book": "job                                ",
    "chapter": "36",
    "verse": "7",
    "text": "Er wendet seine Augen nicht von dem Gerechten und die Konige lalit er sitzen auf dem Thron immerdar, daß sie hoch bleiben.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141202231721986",
    "book": "job                                ",
    "chapter": "36",
    "verse": "8",
    "text": "Und wo Gefangene liegen in Stocken und gebunden mit Stricken elendiglich,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141202788450306",
    "book": "job                                ",
    "chapter": "36",
    "verse": "9",
    "text": "so verkundiget er ihnen, was sie getan haben, und ihre Untugend, daü sie mit Gewalt gefahren haben.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141203338821634",
    "book": "job                                ",
    "chapter": "36",
    "verse": "10",
    "text": "Und offnet ihnen das Ohr zur Zucht und sagt ihnen, daü sie sich von dem Unrechten bekehren sollen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141203902889986",
    "book": "job                                ",
    "chapter": "36",
    "verse": "11",
    "text": "Gehorchen sie und dienen ihm, so werden sie bei guten Tagen alt werden und mit Lust leben.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138141204450934786",
    "book": "job                                ",
    "chapter": "36",
    "verse": "12",
    "text": "Gehorchen sie nicht, so werden sie ins Schwert fallen und vergehen, ehe sie es gewahr werden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138141204997406722",
    "book": "job                                ",
    "chapter": "36",
    "verse": "13",
    "text": "Die Heuchler, wenn sie der Zorn trifft, schreien sie nicht, wenn sie gefangen liegen;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138141205586804738",
    "book": "job                                ",
    "chapter": "36",
    "verse": "14",
    "text": "so wird ihre Seele mit Qual sterben und ihr Leben unter den Hurern.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141206142582786",
    "book": "job                                ",
    "chapter": "36",
    "verse": "15",
    "text": "Aber den Elenden wird er aus seinem Elend erretten und dem Armen das Ohr offnen in Trubsal.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141206685810690",
    "book": "job                                ",
    "chapter": "36",
    "verse": "16",
    "text": "Erwird dich reißen aus dem weiten Rachen der Angst, die keinen Boden hat; und dein Tisch wird Ruhe haben, voll alles Guten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141207222419458",
    "book": "job                                ",
    "chapter": "36",
    "verse": "17",
    "text": "Du aber machst die Sache der Gottlosen gut, dafi ihre Sache und Recht erhalten wird.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141207774101506",
    "book": "job                                ",
    "chapter": "36",
    "verse": "18",
    "text": "Siehe zu, dafi dich nicht vielleicht Zorn beweget habe, jemand zu plagen, Oder grofi Geschenk dich nicht gebeuget habe.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138141208338169858",
    "book": "job                                ",
    "chapter": "36",
    "verse": "19",
    "text": "Meinest du, dafi er deine Gewalt achte, Oder Gold, Oder irgend eine Starke Oder Vermogen?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141208896307202",
    "book": "job                                ",
    "chapter": "36",
    "verse": "20",
    "text": "Du darfst der Nacht nicht begehren, die Leute an ihrem Ortzu überfallen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141209427476482",
    "book": "job                                ",
    "chapter": "36",
    "verse": "21",
    "text": "Hüte dich und kehre dich nicht zum Unrecht, wie du denn vor Elend angefangen hast.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141209980993538",
    "book": "job                                ",
    "chapter": "36",
    "verse": "22",
    "text": "Siehe, Gott ist zu hoch in seiner Kraft; wo ist ein Lehrer, wie er ist?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138141210586120194",
    "book": "job                                ",
    "chapter": "36",
    "verse": "23",
    "text": "Werwill fiber ihn heimsuchen seinen Weg, und wer will zu ihm sagen: Du tust unrecht?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141211147108354",
    "book": "job                                ",
    "chapter": "36",
    "verse": "24",
    "text": "Gedenke, dafi du sein Werk nicht wissest, wie die Leute singen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138141211714191362",
    "book": "job                                ",
    "chapter": "36",
    "verse": "25",
    "text": "Denn alle Menschen sehen das, die Leute schauen's von feme.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141212261974018",
    "book": "job                                ",
    "chapter": "36",
    "verse": "26",
    "text": "Siehe, Gott ist groli und unbekannt; seiner Jahre Zahl kann niemand forschen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138141212855959554",
    "book": "job                                ",
    "chapter": "36",
    "verse": "27",
    "text": "Er macht das Wasser zu kleinen Tropfen und treibt seine Wolken zusammen zum Regen,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141213409476610",
    "book": "job                                ",
    "chapter": "36",
    "verse": "28",
    "text": "daß die Wolken fließen und triefen sehr auf die Menschen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141213948936194",
    "book": "job                                ",
    "chapter": "36",
    "verse": "29",
    "text": "Wenn er vornimmt, die Wolken auszubreiten, wie sein hoch Gezelt,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138141214508056578",
    "book": "job                                ",
    "chapter": "36",
    "verse": "30",
    "text": "siehe, so breitet er aus seinen Blitz fiber dieselben und bedecket alle Enden des Meers.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138141215041224706",
    "book": "job                                ",
    "chapter": "36",
    "verse": "31",
    "text": "Denn damit schreckt er die Leute und gibt doch Speise die Fülle.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141215566725122",
    "book": "job                                ",
    "chapter": "36",
    "verse": "32",
    "text": "Er decket den Blitz wie mit Handen und heilit es doch wiederkommen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141216132988930",
    "book": "job                                ",
    "chapter": "36",
    "verse": "33",
    "text": "Davon zeuget sein Geselle, namlich des Donners Zorn in Wolken.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  }
]