[
  {
    "id": "1138141150484201474",
    "book": "job                                ",
    "chapter": "33",
    "verse": "1",
    "text": "Hore doch, Hiob, meine Rede und merke auf alle meine Worte!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141151049613314",
    "book": "job                                ",
    "chapter": "33",
    "verse": "2",
    "text": "Siehe, ich tue meinen Mund auf, und meine Zunge redet in meinem Munde.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138141151608832002",
    "book": "job                                ",
    "chapter": "33",
    "verse": "3",
    "text": "Mein Herz soil recht reden, und meine Lippen sollen den reinen Verstand sagen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141152168509442",
    "book": "job                                ",
    "chapter": "33",
    "verse": "4",
    "text": "Der Geist Gottes hat mich gemacht, und der Odem des Allmachtigen hat mir das Leben gegeben.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141152704561154",
    "book": "job                                ",
    "chapter": "33",
    "verse": "5",
    "text": "Kannst du, so antworte mir; schicke dich gegen mich und stelle dich!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141153258307586",
    "book": "job                                ",
    "chapter": "33",
    "verse": "6",
    "text": "Siehe ich bin Gottes ebensowohl als du, und aus Leimen bin ich auch gemacht.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141153798848514",
    "book": "job                                ",
    "chapter": "33",
    "verse": "7",
    "text": "Doch du darfst vor mir nicht erschrecken, und meine Hand soli dir nicht zu schwer sein.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138141154353971202",
    "book": "job                                ",
    "chapter": "33",
    "verse": "8",
    "text": "Du hast geredet vor meinen Ohren, die Stimme deiner Rede mulite ich horen:                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138141154907160578",
    "book": "job                                ",
    "chapter": "33",
    "verse": "9",
    "text": "Ich bin rein, ohne Missetat, unschuldig und habe keine Sunde.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138141155464282114",
    "book": "job                                ",
    "chapter": "33",
    "verse": "10",
    "text": "Siehe, er hat eine Sache wider mich funden, darum achtet er mich fur seinen Feind.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141156002562050",
    "book": "job                                ",
    "chapter": "33",
    "verse": "11",
    "text": "Er hat meinen Fuli in Stock gelegt und hat alle meine Wege verwahret.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138141156585439234",
    "book": "job                                ",
    "chapter": "33",
    "verse": "12",
    "text": "Siehe, eben daraus schlielie ich wider dich, daß du nicht recht bist; denn Gott ist mehr weder ein Mensch.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141157162647554",
    "book": "job                                ",
    "chapter": "33",
    "verse": "13",
    "text": "Warum willstdu mit ihm zanken, daß erdir nicht Rechenschaft gibt alles seines Tuns?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141157706104834",
    "book": "job                                ",
    "chapter": "33",
    "verse": "14",
    "text": "Denn wenn Gott einmal etwas beschlielit, so bedenket er's nicht erst her nach.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141158290653186",
    "book": "job                                ",
    "chapter": "33",
    "verse": "15",
    "text": "Im Traum des Gesichts in der Nacht, wenn der Schlaf auf die Leute fallt, wenn sie schlafen auf dem Bette,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141158849118210",
    "book": "job                                ",
    "chapter": "33",
    "verse": "16",
    "text": "da offnet er das Ohr der Leute und schreckt sie und zuchtiget sie,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141159404961794",
    "book": "job                                ",
    "chapter": "33",
    "verse": "17",
    "text": "daß er den Menschen von seinem Vorhaben wende und beschirme ihn vor Hoffart.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138141159952678914",
    "book": "job                                ",
    "chapter": "33",
    "verse": "18",
    "text": "Und verschonet seiner Seele vor dem Verderben und seines Lebens, daß es nicht ins Schwert falle.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141160532738050",
    "book": "job                                ",
    "chapter": "33",
    "verse": "19",
    "text": "Er straft ihn mit Schmerzen auf seinem Bette und alle seine Gebeine heftig;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138141161077342210",
    "book": "job                                ",
    "chapter": "33",
    "verse": "20",
    "text": "und richtet ihm sein Leben so zu, daß ihm vor der Speise ekelt, und seine Seele, daß sie nicht Lust zu essen hat.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     "
  },
  {
    "id": "1138141161622437890",
    "book": "job                                ",
    "chapter": "33",
    "verse": "21",
    "text": "Sein Fleisch verschwindet, daß er nicht wohl sehen mag, und seine Beine werden zerschlagen, daß man sie nicht gerne ansiehet,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138141162169008130",
    "book": "job                                ",
    "chapter": "33",
    "verse": "22",
    "text": "daß seine Seele nahet zum Verderben und sein Leben zu den Toten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138141162750935042",
    "book": "job                                ",
    "chapter": "33",
    "verse": "23",
    "text": "So dann ein Engel, einer aus tausend, mit ihm redet, zu verkundigen dem Menschen, wie er solle recht tun,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141163299766274",
    "book": "job                                ",
    "chapter": "33",
    "verse": "24",
    "text": "so wird er ihm gnadig sein und sagen: Er soil erloset werden, daß er nicht hinunterfahre ins Verderben; denn ich habe eine Versohnung funden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141163860590594",
    "book": "job                                ",
    "chapter": "33",
    "verse": "25",
    "text": "Sein Fleisch grüne wieder wie in der Jugend, und lali ihn wieder jung werden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141164401426434",
    "book": "job                                ",
    "chapter": "33",
    "verse": "26",
    "text": "Er wird Gott bitten; der wird ihm Gnade erzeigen und wird sein Antlitz sehen lassen mit Freuden und wird dem Menschen nach seiner Gerechtigkeit vergelten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138141164951076866",
    "book": "job                                ",
    "chapter": "33",
    "verse": "27",
    "text": "Er wird vor den Leuten bekennen und sagen: Ich wollte gesundiget und das Recht verkehret haben, aber es hatte mir nichts genutzet.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141165512949762",
    "book": "job                                ",
    "chapter": "33",
    "verse": "28",
    "text": "Er hat meine Seele erloset, daß sie nicht fuhre ins Verderben, sondern mein Leben das Licht sahe.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138141166131478530",
    "book": "job                                ",
    "chapter": "33",
    "verse": "29",
    "text": "Siehe, das alles tut Gott zwei Oder dreimal mit einem jeglichen,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138141166663368706",
    "book": "job                                ",
    "chapter": "33",
    "verse": "30",
    "text": "daß er seine Seele herumhole aus dem Verderben und erleuchte ihn mit dem Licht der Lebendigen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138141167240118274",
    "book": "job                                ",
    "chapter": "33",
    "verse": "31",
    "text": "Merke auf, Hiob, und hore mirzu, und schweige, daß ich rede!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138141167792390146",
    "book": "job                                ",
    "chapter": "33",
    "verse": "32",
    "text": "Hast du aber was zu sagen, so antworte mir; sage her, bist du recht, ich will's gerne horen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138141168364781570",
    "book": "job                                ",
    "chapter": "33",
    "verse": "33",
    "text": "Hast du aber nichts, so hore mir zu und schweige, ich will dich die Weisheit lehren.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  }
]