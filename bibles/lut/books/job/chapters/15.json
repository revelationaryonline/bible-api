[
  {
    "id": "1138140902988218370",
    "book": "job                                ",
    "chapter": "15",
    "verse": "1",
    "text": "Da antwortete Eliphas von Theman und sprach:                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138140903537737730",
    "book": "job                                ",
    "chapter": "15",
    "verse": "2",
    "text": "Soli ein weiser Mann so aufgeblasene Worte reden und seinen Bauch so blahen mit losen Reden?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138140904119730178",
    "book": "job                                ",
    "chapter": "15",
    "verse": "3",
    "text": "Du strafest mit Worten, die nicht taugen, und dein Reden ist kein nütze.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138140904719384578",
    "book": "job                                ",
    "chapter": "15",
    "verse": "4",
    "text": "Du hast die Furcht fahren lassen und redest zu verachtlich vor Gott.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138140905262415874",
    "book": "job                                ",
    "chapter": "15",
    "verse": "5",
    "text": "Denn deine Missetat lehret deinen Mund also, und hast erwahlet eine schalkhafte Zunge.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138140905822584834",
    "book": "job                                ",
    "chapter": "15",
    "verse": "6",
    "text": "Dein Mund wird dich verdammen, und nicht ich; deine Lippen sollen dir antworten.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138140906387013634",
    "book": "job                                ",
    "chapter": "15",
    "verse": "7",
    "text": "Bist du der erste Mensch geboren? Bist du vor alien Hugeln empfangen?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138140906938990594",
    "book": "job                                ",
    "chapter": "15",
    "verse": "8",
    "text": "Hast du Gottes heimlichen Rat gehoret? und ist die Weisheit selbst geringer denn du?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138140907484348418",
    "book": "job                                ",
    "chapter": "15",
    "verse": "9",
    "text": "Was weiftt du, das wir nicht wissen? Was verstehest du, das nicht bei uns sei?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138140908014272514",
    "book": "job                                ",
    "chapter": "15",
    "verse": "10",
    "text": "Es sind Graue und Alte unter uns, die langer gelebt haben denn deine Vater.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138140908567166978",
    "book": "job                                ",
    "chapter": "15",
    "verse": "11",
    "text": "Sollten Gottes Trostungen so geringe vor dir gelten? Aber du hast irgend noch ein heimlich Stuck bei dir.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138140909115080706",
    "book": "job                                ",
    "chapter": "15",
    "verse": "12",
    "text": "Was nimmt dein Herz vor? Was siehest du so stolz?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138140909685047298",
    "book": "job                                ",
    "chapter": "15",
    "verse": "13",
    "text": "Was setztsich dein Mut wider Gott, daß du solche Rede aus deinem Munde lassest?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138140910268088322",
    "book": "job                                ",
    "chapter": "15",
    "verse": "14",
    "text": "Was ist ein Mensch, daß er sollte rein sein, und daß der sollte gerecht sein, der vom Weibe geboren ist?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138140910814986242",
    "book": "job                                ",
    "chapter": "15",
    "verse": "15",
    "text": "Siehe, unter seinen Heiligen ist keiner ohne Tadel, und die Himmel sind nicht rein vor ihm.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138140911388884994",
    "book": "job                                ",
    "chapter": "15",
    "verse": "16",
    "text": "Wie viel mehr ein Mensch, der ein Greuel und schnode ist, der Unrecht sauft wie Wasser.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138140911968518146",
    "book": "job                                ",
    "chapter": "15",
    "verse": "17",
    "text": "Ich will dir'szeigen, hore mirzu; und will dir erzahlen was ich gesehen habe,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138140912491200514",
    "book": "job                                ",
    "chapter": "15",
    "verse": "18",
    "text": "was die Weisen gesagt haben, und ihren Vatern nicht verhohlen gewesen ist,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138140913035280386",
    "book": "job                                ",
    "chapter": "15",
    "verse": "19",
    "text": "welchen allein das Land gegeben ist, daß kein Fremder durch sie gehen muft.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138140913587552258",
    "book": "job                                ",
    "chapter": "15",
    "verse": "20",
    "text": "Der Gottlose bebet sein Leben lang; und dem Tyrannen ist die Zahl seiner Jahre verborgen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138140914139496450",
    "book": "job                                ",
    "chapter": "15",
    "verse": "21",
    "text": "Was er horet, das schrecket ihn; und wenn's gleich Friede ist, furchtet er sich, der Verderber komme;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138140914697502722",
    "book": "job                                ",
    "chapter": "15",
    "verse": "22",
    "text": "glaubt nicht, daß er moge dem Unglück entrinnen, und versiehet sich immer des Schwerts.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138140915233226754",
    "book": "job                                ",
    "chapter": "15",
    "verse": "23",
    "text": "Erzeucht hin und hernach Brot und dunket ihn immer, die Zeit seines Unglucks sei vorhanden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138140915768721410",
    "book": "job                                ",
    "chapter": "15",
    "verse": "24",
    "text": "Angst und Not schrecken ihn und schlagen ihn nieder als ein Konig mit einem Heer.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138140916326662146",
    "book": "job                                ",
    "chapter": "15",
    "verse": "25",
    "text": "Denn er hat seine Hand wider Gott gestrecket und wider den Allmachtigen sich gestraubet.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138140916879294466",
    "book": "job                                ",
    "chapter": "15",
    "verse": "26",
    "text": "Er lauft mit dem Kopf an ihn und ficht halsstarriglich wider ihn.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138140917435465730",
    "book": "job                                ",
    "chapter": "15",
    "verse": "27",
    "text": "Er brustet sich wie ein fetter Wanst und macht sich fett und dick.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138140917979119618",
    "book": "job                                ",
    "chapter": "15",
    "verse": "28",
    "text": "Erwird aberwohnen in verstdrten Stadten, da keine Haupter sind, sondern auf einem Haufen liegen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138140918523756546",
    "book": "job                                ",
    "chapter": "15",
    "verse": "29",
    "text": "Erwird nicht reich bleiben, und sein Gut wird nicht bestehen, und sein Gluck wird sich nicht ausbreiten im Lande.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138140919121215490",
    "book": "job                                ",
    "chapter": "15",
    "verse": "30",
    "text": "Unfall wird nicht von ihm lassen. Die Flamme wird seine Zweige verdorren und durch den Odem ihres Mundes ihn wegfressen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138140919662608386",
    "book": "job                                ",
    "chapter": "15",
    "verse": "31",
    "text": "Erwird nicht bestehen, denn er ist in seinem eiteln Dunkel betrogen, und eitel wird sein Lohn werden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138140920177623042",
    "book": "job                                ",
    "chapter": "15",
    "verse": "32",
    "text": "Erwird ein Ende nehmen, wenn's ihm uneben ist, und sein Zweig wird nicht grunen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138140920731631618",
    "book": "job                                ",
    "chapter": "15",
    "verse": "33",
    "text": "Erwird abgerissen werden wie eine unzeitige Traube vom Weinstock, und wie ein Olbaum seine Blüte abwirft.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138140921294192642",
    "book": "job                                ",
    "chapter": "15",
    "verse": "34",
    "text": "Denn der Heuchler Versammlung wird einsam bleiben, und das Feuerwird die Hütten fressen, die Geschenke nehmen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138140921829195778",
    "book": "job                                ",
    "chapter": "15",
    "verse": "35",
    "text": "Er gehet schwanger mit Unglück und gebiert Mühe, und ihr Bauch bringet Fehl.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  }
]