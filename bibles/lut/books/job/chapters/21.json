[
  {
    "id": "1138140987158265858",
    "book": "job                                ",
    "chapter": "21",
    "verse": "1",
    "text": "Hiob antwortete und sprach:                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138140987708866562",
    "book": "job                                ",
    "chapter": "21",
    "verse": "2",
    "text": "Horet doch zu meiner Rede und lasset euch raten!                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138140988252618754",
    "book": "job                                ",
    "chapter": "21",
    "verse": "3",
    "text": "Vertraget mich, dati ich auch rede, und spottet danach mein.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            "
  },
  {
    "id": "1138140988794404866",
    "book": "job                                ",
    "chapter": "21",
    "verse": "4",
    "text": "Handele ich denn miteinem Menschen, daß mein Mut hierin nicht sollte unwillig sein?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138140989334126594",
    "book": "job                                ",
    "chapter": "21",
    "verse": "5",
    "text": "Kehret euch her zu mir; ihr werdet sauer sehen und die Hand aufs Maul legen müssen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138140989892919298",
    "book": "job                                ",
    "chapter": "21",
    "verse": "6",
    "text": "Wenn ich daran gedenke, so erschrecke ich, und Zittern kommt mein Fleisch an.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           "
  },
  {
    "id": "1138140990475272194",
    "book": "job                                ",
    "chapter": "21",
    "verse": "7",
    "text": "Warum leben denn die Gottlosen, werden alt und nehmen zu mit Gutern?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138140991012143106",
    "book": "job                                ",
    "chapter": "21",
    "verse": "8",
    "text": "Ihr Same ist sicher urn sie her, und ihre Nachkommlinge sind bei ihnen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138140991534923778",
    "book": "job                                ",
    "chapter": "21",
    "verse": "9",
    "text": "Ihr Haus hat Frieden vorder Furcht, und Gottes Rute ist nicht über ihnen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              "
  },
  {
    "id": "1138140992099844098",
    "book": "job                                ",
    "chapter": "21",
    "verse": "10",
    "text": "Seine Ochsen lasset man zu, und miftrat ihm nicht; seine Kuh kalbet und ist nicht unfruchtbar.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138140992617512962",
    "book": "job                                ",
    "chapter": "21",
    "verse": "11",
    "text": "Ihre jungen Kinder gehen aus wie eine Herde, und ihre Kinder lecken.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138140993176928258",
    "book": "job                                ",
    "chapter": "21",
    "verse": "12",
    "text": "Sie jauchzen mit Pauken und Harfen und sind frohlich mit Pfeifen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138140993728675842",
    "book": "job                                ",
    "chapter": "21",
    "verse": "13",
    "text": "Sie werden alt bei guten Tagen und erschrecken kaum einen Augenblick vorder Holle,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138140994275180546",
    "book": "job                                ",
    "chapter": "21",
    "verse": "14",
    "text": "die doch sagen zu Gott: Hebe dich von uns, wir wollen von deinen Wegen nicht wissen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138140994886172674",
    "book": "job                                ",
    "chapter": "21",
    "verse": "15",
    "text": "Wer ist der Allmachtige, daß wir ihm dienen sollten, Oder was sind wir gebessert, so wir ihn anrufen?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138140995433529346",
    "book": "job                                ",
    "chapter": "21",
    "verse": "16",
    "text": "Aber siehe, ihr Gut stehet nicht in ihren Handen; darum soil der Gottlosen Sinn feme von mir sein.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      "
  },
  {
    "id": "1138140995994746882",
    "book": "job                                ",
    "chapter": "21",
    "verse": "17",
    "text": "Wie wird die Leuchte der Gottlosen verloschen und ihr Ungluck über sie kommen! Er wird Herzeleid austeilen in seinem Zorn.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
    "id": "1138140996538433538",
    "book": "job                                ",
    "chapter": "21",
    "verse": "18",
    "text": "Sie werden sein wie Stoppeln vor dem Winde und wie Spreu, die der Sturmwind wegführet.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138140997092179970",
    "book": "job                                ",
    "chapter": "21",
    "verse": "19",
    "text": "Gott behalt desselben Ungluck auf seine Kinder. Wenn er's ihm vergelten wird, so wird man's inne werden.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138140997696487426",
    "book": "job                                ",
    "chapter": "21",
    "verse": "20",
    "text": "Seine Augen werden sein Verderben sehen, und vom Grimm des Allmachtigen wird er trinken.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138140998211796994",
    "book": "job                                ",
    "chapter": "21",
    "verse": "21",
    "text": "Denn wer wird Gefallen haben an seinem Hause nach ihm? Und die Zahl seiner Monden wird kaum halb bleiben.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138140998745817090",
    "book": "job                                ",
    "chapter": "21",
    "verse": "22",
    "text": "Wer will Gott lehren, der auch die Hohen richtet?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       "
  },
  {
    "id": "1138140999325515778",
    "book": "job                                ",
    "chapter": "21",
    "verse": "23",
    "text": "Dieser stirbt frisch und gesund in allem Reichtum und voller Genüge;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138140999870906370",
    "book": "job                                ",
    "chapter": "21",
    "verse": "24",
    "text": "sein Melkfaft ist voll Milch, und seine Gebeine werden gemastet mit Mark;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141000461975554",
    "book": "job                                ",
    "chapter": "21",
    "verse": "25",
    "text": "jener aber stirbt mit betrubter Seele und hat nie mit Freuden gegessen;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 "
  },
  {
    "id": "1138141001020080130",
    "book": "job                                ",
    "chapter": "21",
    "verse": "26",
    "text": "und liegen gleich miteinander in der Erde, und Würmer decken sie zu.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138141001569042434",
    "book": "job                                ",
    "chapter": "21",
    "verse": "27",
    "text": "Siehe, ich kenne eure Gedanken wohl und euer frevel Vornehmen wider mich.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  },
  {
    "id": "1138141002102767618",
    "book": "job                                ",
    "chapter": "21",
    "verse": "28",
    "text": "Denn ihr sprechet: Wo ist das Haus des Fursten, und wo ist die Hutte, da die Gottlosen wohneten?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        "
  },
  {
    "id": "1138141002672472066",
    "book": "job                                ",
    "chapter": "21",
    "verse": "29",
    "text": "Redet ihr doch davon wie der gemeine Pobel und merket nicht, was jener Wesen bedeutet.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138141003227430914",
    "book": "job                                ",
    "chapter": "21",
    "verse": "30",
    "text": "Denn der Bose wird behalten auf den Tag des Verderbens, und auf den Tag des Grimms bleibet er.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          "
  },
  {
    "id": "1138141003783143426",
    "book": "job                                ",
    "chapter": "21",
    "verse": "31",
    "text": "Wer will sagen, was erverdienet, wenn man's aufterlich ansiehet? Wer will ihm vergelten, was er tut?                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    "
  },
  {
    "id": "1138141004342722562",
    "book": "job                                ",
    "chapter": "21",
    "verse": "32",
    "text": "Aber erwird zum Grabe gerissen und muft bleiben bei dem Haufen.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         "
  },
  {
    "id": "1138141004909019138",
    "book": "job                                ",
    "chapter": "21",
    "verse": "33",
    "text": "Es gefiel ihm wohl der Schlamm des Bachs, und alle Menschen werden ihm nachgezogen; und derer, die vor ihm gewesen sind, ist keine Zahl.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                "
  },
  {
    "id": "1138141005477314562",
    "book": "job                                ",
    "chapter": "21",
    "verse": "34",
    "text": "Wie trostet ihr mich so vergeblich, und eure Antwort findet sich unrecht.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               "
  }
]