    "text": "Platja, Hanan, Anaja,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   "
  },
  {
    "id": "1138140549819301890",
    "book": "nehemiah                           ",
    "chapter": "10",
    "verse": "24",
    "text": "Hosea, Hananja, Hasub,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  "
  },
  {
    "id": "1138140550350012418",
