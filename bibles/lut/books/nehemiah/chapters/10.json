[
  {
    "id": "1138140536988958722",
    "book": "nehemiah                           ",
    "chapter": "10",
    "verse": "1",
    "text": "Und in diesem allem machen wireinen festen Bund und schreiben und lassen's unsere Fursten, Leviten und Priester versiegeln.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             "
  },
  {
