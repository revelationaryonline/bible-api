    "verse": "13",
    "text": "Hizo alejar de mí mis hermanos,"
  },
  {
    "id": 16638,
    "book": "job                                     ",
    "chapter": "19",
    "verse": "14",
    "text": "Mis parientes se detuvieron,"
  },
  {
    "id": 16639,
    "book": "job                                     ",
    "chapter": "19",
