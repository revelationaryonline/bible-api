[
  {
    "id": 25192,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "1",
    "text": "BIENAVENTURADOS los 119.1 Gn. 17.1.perfectos de camino;"
  },
  {
    "id": 25193,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "2",
    "text": "Bienaventurados los que guardan sus testimonios,"
  },
  {
    "id": 25194,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "3",
    "text": "Pues no hacen iniquidad"
  },
  {
    "id": 25195,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "4",
    "text": "Tú encargaste"
  },
  {
    "id": 25196,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "5",
    "text": "¡Ojalá fuesen ordenados mis caminos"
  },
  {
    "id": 25197,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "6",
    "text": "Entonces no sería yo avergonzado,"
  },
  {
    "id": 25198,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "7",
    "text": "Te alabaré con rectitud de corazón,"
  },
  {
    "id": 25199,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "8",
    "text": "Tus estatutos guardaré:"
  },
  {
    "id": 25200,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "9",
    "text": "¿Con qué limpiará el joven su camino?"
  },
  {
    "id": 25201,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "10",
    "text": "Con todo mi corazón te he buscado:"
  },
  {
    "id": 25202,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "11",
    "text": "En mi corazón he guardado tus dichos,"
  },
  {
    "id": 25203,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "12",
    "text": "Bendito tú, oh Jehová:"
  },
  {
    "id": 25204,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "13",
    "text": "Con mis labios he contado"
  },
  {
    "id": 25205,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "14",
    "text": "Heme gozado en el camino de tus testimonios,"
  },
  {
    "id": 25206,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "15",
    "text": "En tus mandamientos meditaré,"
  },
  {
    "id": 25207,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "16",
    "text": "Recrearéme en tus estatutos:"
  },
  {
    "id": 25208,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "17",
    "text": "Haz bien á tu siervo; que viva"
  },
  {
    "id": 25209,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "18",
    "text": "Abre mis ojos, y miraré"
  },
  {
    "id": 25210,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "19",
    "text": "Advenedizo soy yo en la tierra:"
  },
  {
    "id": 25211,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "20",
    "text": "Quebrantada está mi alma de desear"
  },
  {
    "id": 25212,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "21",
    "text": "Destruiste á los soberbios malditos,"
  },
  {
    "id": 25213,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "22",
    "text": "Aparta de mí oprobio y menosprecio;"
  },
  {
    "id": 25214,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "23",
    "text": "Príncipes también se sentaron y hablaron contra mí:"
  },
  {
    "id": 25215,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "24",
    "text": "Pues 119.24 vers. 77,92,143,174.tus testimonios son mis deleites,"
  },
  {
    "id": 25216,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "25",
    "text": "Pegóse al polvo mi alma:"
  },
  {
    "id": 25217,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "26",
    "text": "Mis caminos te conté, y me has respondido:"
  },
  {
    "id": 25218,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "27",
    "text": "Hazme entender el camino de tus mandamientos,"
  },
  {
    "id": 25219,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "28",
    "text": "Deshácese mi alma de ansiedad:"
  },
  {
    "id": 25220,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "29",
    "text": "Aparta de mí camino de mentira;"
  },
  {
    "id": 25221,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "30",
    "text": "Escogí el camino de la verdad;"
  },
  {
    "id": 25222,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "31",
    "text": "Allegádome he á tus testimonios;"
  },
  {
    "id": 25223,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "32",
    "text": "Por el camino de tus mandamientos correré,"
  },
  {
    "id": 25224,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "33",
    "text": "Enséñame, oh Jehová, el camino de tus estatutos,"
  },
  {
    "id": 25225,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "34",
    "text": "Dame entendimiento, y guardaré tu ley;"
  },
  {
    "id": 25226,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "35",
    "text": "Guíame por la senda de tus mandamientos;"
  },
  {
    "id": 25227,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "36",
    "text": "Inclina mi corazón á tus testimonios,"
  },
  {
    "id": 25228,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "37",
    "text": "Aparta mis ojos, que 119.37 Pr. 23.5.no vean la vanidad;"
  },
  {
    "id": 25229,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "38",
    "text": "Confirma tu palabra á tu siervo,"
  },
  {
    "id": 25230,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "39",
    "text": "Quita de mí el oprobio que he temido:"
  },
  {
    "id": 25231,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "40",
    "text": "He aquí yo he 119.40 ver. 20.codiciado tus mandamientos:"
  },
  {
    "id": 25232,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "41",
    "text": "Y venga á mí tu misericordia, oh Jehová;"
  },
  {
    "id": 25233,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "42",
    "text": "Y daré por respuesta á mi avergonzador,"
  },
  {
    "id": 25234,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "43",
    "text": "Y no quites de mi boca en nigún tiempo la palabra de verdad;"
  },
  {
    "id": 25235,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "44",
    "text": "Y guardaré tu ley siempre,"
  },
  {
    "id": 25236,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "45",
    "text": "Y andaré en anchura,"
  },
  {
    "id": 25237,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "46",
    "text": "Y hablaré de tus testimonios delante de los reyes,"
  },
  {
    "id": 25238,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "47",
    "text": "Y deleitaréme en tus mandamientos,"
  },
  {
    "id": 25239,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "48",
    "text": "Alzaré asimismo mis manos á tus mandamientos que amé;"
  },
  {
    "id": 25240,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "49",
    "text": "Acuérdate de la palabra dada á tu siervo,"
  },
  {
    "id": 25241,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "50",
    "text": "Esta es mi 119.50 Ro. 15.4.consuelo en mi aflicción:"
  },
  {
    "id": 25242,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "51",
    "text": "Los soberbios se burlaron mucho de mí:"
  },
  {
    "id": 25243,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "52",
    "text": "Acordéme, oh Jehová, de tus juicios antiguos,"
  },
  {
    "id": 25244,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "53",
    "text": "Horror se apoderó de mí, á causa"
  },
  {
    "id": 25245,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "54",
    "text": "Cánticos me fueron tus estatutos"
  },
  {
    "id": 25246,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "55",
    "text": "Acordéme en la noche de tu nombre, oh Jehová,"
  },
  {
    "id": 25247,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "56",
    "text": "Esto tuve,"
  },
  {
    "id": 25248,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "57",
    "text": "Mi porción, oh Jehová,"
  },
  {
    "id": 25249,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "58",
    "text": "Tu presencia supliqué de todo corazón:"
  },
  {
    "id": 25250,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "59",
    "text": "Consideré mis caminos,"
  },
  {
    "id": 25251,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "60",
    "text": "Apresuréme, y no me retardé"
  },
  {
    "id": 25252,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "61",
    "text": "Compañía de impíos me han robado:"
  },
  {
    "id": 25253,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "62",
    "text": "A media noche me levantaba á alabarte"
  },
  {
    "id": 25254,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "63",
    "text": "Compañero soy yo de todos los que te temieren"
  },
  {
    "id": 25255,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "64",
    "text": "De tu misericordia, oh Jehová, está llena la tierra:"
  },
  {
    "id": 25256,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "65",
    "text": "Bien has hecho con tu siervo,"
  },
  {
    "id": 25257,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "66",
    "text": "Enséñame bondad de sentido y sabiduría;"
  },
  {
    "id": 25258,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "67",
    "text": "Antes que fuera yo humillado, descarriado andaba;"
  },
  {
    "id": 25259,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "68",
    "text": "Bueno eres tú, y bienhechor:"
  },
  {
    "id": 25260,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "69",
    "text": "Contra mí 119.69 Job 13.4. Sal. 109.2.forjaron mentira los soberbios:"
  },
  {
    "id": 25261,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "70",
    "text": "Engrasóse el corazón de ellos como sebo;"
  },
  {
    "id": 25262,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "71",
    "text": "Bueno me es haber sido humillado,"
  },
  {
    "id": 25263,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "72",
    "text": "Mejor me es la ley de tu boca,"
  },
  {
    "id": 25264,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "73",
    "text": "Tus manos me hicieron y me formaron:"
  },
  {
    "id": 25265,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "74",
    "text": "Los que te temen, me verán, y se alegrarán;"
  },
  {
    "id": 25266,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "75",
    "text": "Conozco, oh Jehová, que tus juicios son justicia,"
  },
  {
    "id": 25267,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "76",
    "text": "Sea ahora tu misericordia para consolarme,"
  },
  {
    "id": 25268,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "77",
    "text": "Vengan á mí tus misericordias, y viva;"
  },
  {
    "id": 25269,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "78",
    "text": "Sean avergonzados los soberbios, porque sin causa me han calumniado:"
  },
  {
    "id": 25270,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "79",
    "text": "Tórnense á mí los que te temen"
  },
  {
    "id": 25271,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "80",
    "text": "Sea mi corazón íntegro en tus estatutos;"
  },
  {
    "id": 25272,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "81",
    "text": "Desfallece 119.81 Sal. 73.26 y 84.2.mi alma por tu salud,"
  },
  {
    "id": 25273,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "82",
    "text": "Desfallecieron mis ojos por tu palabra,"
  },
  {
    "id": 25274,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "83",
    "text": "Porque 119.83 Job 30.30.estoy como el odre al humo;"
  },
  {
    "id": 25275,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "84",
    "text": "¿Cuántos son los días de tu siervo?"
  },
  {
    "id": 25276,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "85",
    "text": "Los soberbios me han cavado hoyos;"
  },
  {
    "id": 25277,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "86",
    "text": "Todos tus mandamientos son verdad:"
  },
  {
    "id": 25278,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "87",
    "text": "Casi me han echado por tierra:"
  },
  {
    "id": 25279,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "88",
    "text": "Vivifícame conforme á tu misericordia;"
  },
  {
    "id": 25280,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "89",
    "text": "Para siempre, oh Jehová,"
  },
  {
    "id": 25281,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "90",
    "text": "Por generación y generación es tu 119.90 Sal. 89.1.verdad:"
  },
  {
    "id": 25282,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "91",
    "text": "Por tu ordenación perseveran hasta hoy 119.91 Jer. 33.25.las cosas criadas;"
  },
  {
    "id": 25283,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "92",
    "text": "Si tu ley no hubiese sido mis delicias,"
  },
  {
    "id": 25284,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "93",
    "text": "Nunca jamás me olvidaré de tus mandamientos;"
  },
  {
    "id": 25285,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "94",
    "text": "Tuyo soy yo, guárdame;"
  },
  {
    "id": 25286,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "95",
    "text": "Los impíos me han aguardado para destruirme:"
  },
  {
    "id": 25287,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "96",
    "text": "A toda perfección he visto fin:"
  },
  {
    "id": 25288,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "97",
    "text": "¡Cuánto amo yo tu ley!"
  },
  {
    "id": 25289,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "98",
    "text": "Me has hecho más sabio que mis enemigos 119.98 Dt. 4.6.con tus mandamientos;"
  },
  {
    "id": 25290,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "99",
    "text": "Más que todos mis enseñadores he entendido:"
  },
  {
    "id": 25291,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "100",
    "text": "Más que los viejos he entendido,"
  },
  {
    "id": 25292,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "101",
    "text": "De todo mal camino contuve mis pies,"
  },
  {
    "id": 25293,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "102",
    "text": "No me aparté de tus juicios;"
  },
  {
    "id": 25294,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "103",
    "text": "¡Cuán dulces son á mi paladar tus palabras!"
  },
  {
    "id": 25295,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "104",
    "text": "De tus mandamientos he adquirido inteligencia:"
  },
  {
    "id": 25296,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "105",
    "text": "Lámpara es á mis pies tu palabra,"
  },
  {
    "id": 25297,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "106",
    "text": "Juré y ratifiqué"
  },
  {
    "id": 25298,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "107",
    "text": "Afligido estoy en gran manera: oh Jehová,"
  },
  {
    "id": 25299,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "108",
    "text": "Ruégote, oh Jehová, te sean agradables 119.108 Os. 14.2. He. 13.15.los sacrificios voluntarios de mi boca;"
  },
  {
    "id": 25300,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "109",
    "text": "De continuo está mi alma en mi mano:"
  },
  {
    "id": 25301,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "110",
    "text": "Pusiéronme lazo los impíos:"
  },
  {
    "id": 25302,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "111",
    "text": "Por heredad he tomado tus testimonios para siempre;"
  },
  {
    "id": 25303,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "112",
    "text": "Mi corazón incliné á poner por obra tus estatutos"
  },
  {
    "id": 25304,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "113",
    "text": "Los pensamientos vanos aborrezco;"
  },
  {
    "id": 25305,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "114",
    "text": "Mi escondedero y mi 119.114 Sal. 84.9.escudo eres tú:"
  },
  {
    "id": 25306,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "115",
    "text": "Apartaos de mí, malignos;"
  },
  {
    "id": 25307,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "116",
    "text": "Susténtame conforme á tu palabra, y viviré:"
  },
  {
    "id": 25308,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "117",
    "text": "Sosténme, y seré salvo;"
  },
  {
    "id": 25309,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "118",
    "text": "Hollaste á todos los que se desvían de tus estatutos:"
  },
  {
    "id": 25310,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "119",
    "text": "Como escorias hiciste consumir á todos los impíos de la tierra:"
  },
  {
    "id": 25311,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "120",
    "text": "Mi carne se ha extremecido por temor de ti;"
  },
  {
    "id": 25312,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "121",
    "text": "Juicio y justicia he hecho;"
  },
  {
    "id": 25313,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "122",
    "text": "Responde por tu siervo para bien:"
  },
  {
    "id": 25314,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "123",
    "text": "Mis ojos desfallecieron por tu salud,"
  },
  {
    "id": 25315,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "124",
    "text": "Haz con tu siervo según tu misericordia,"
  },
  {
    "id": 25316,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "125",
    "text": "Tu siervo soy yo, dame entendimiento;"
  },
  {
    "id": 25317,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "126",
    "text": "Tiempo es de hacer, oh Jehová;"
  },
  {
    "id": 25318,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "127",
    "text": "Por eso he amado tus mandamientos"
  },
  {
    "id": 25319,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "128",
    "text": "Por eso todos los mandamientos de todas las cosas estimé rectos:"
  },
  {
    "id": 25320,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "129",
    "text": "Maravillosos son tus testimonios:"
  },
  {
    "id": 25321,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "130",
    "text": "El principio de tus palabras alumbra;"
  },
  {
    "id": 25322,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "131",
    "text": "Mi boca abrí y suspiré;"
  },
  {
    "id": 25323,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "132",
    "text": "Mírame, y ten misericordia de mí,"
  },
  {
    "id": 25324,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "133",
    "text": "Ordena mis pasos con tu palabra;"
  },
  {
    "id": 25325,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "134",
    "text": "Redímeme de la violencia de los hombres;"
  },
  {
    "id": 25326,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "135",
    "text": "Haz que tu rostro resplandezca sobre tu siervo;"
  },
  {
    "id": 25327,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "136",
    "text": "Ríos de agua descendieron de mis ojos,"
  },
  {
    "id": 25328,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "137",
    "text": "Justo eres tú, oh Jehová,"
  },
  {
    "id": 25329,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "138",
    "text": "Tus testimonios, que has recomendado,"
  },
  {
    "id": 25330,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "139",
    "text": "Mi celo me ha consumido;"
  },
  {
    "id": 25331,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "140",
    "text": "Sumamente acendrada es tu palabra;"
  },
  {
    "id": 25332,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "141",
    "text": "Pequeño soy yo y desechado;"
  },
  {
    "id": 25333,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "142",
    "text": "Tu justicia es justicia eterna,"
  },
  {
    "id": 25334,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "143",
    "text": "Aflicción y angustia me hallaron:"
  },
  {
    "id": 25335,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "144",
    "text": "Justicia eterna son tus testimonios;"
  },
  {
    "id": 25336,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "145",
    "text": "Clamé con todo mi corazón; respóndeme, Jehová,"
  },
  {
    "id": 25337,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "146",
    "text": "A ti clamé; sálvame,"
  },
  {
    "id": 25338,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "147",
    "text": "Anticipéme al alba, y clamé:"
  },
  {
    "id": 25339,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "148",
    "text": "Previnieron mis ojos las vigilias de la noche,"
  },
  {
    "id": 25340,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "149",
    "text": "Oye mi voz conforme á tu misericordia;"
  },
  {
    "id": 25341,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "150",
    "text": "Acercáronse á la maldad los que me persiguen;"
  },
  {
    "id": 25342,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "151",
    "text": "Cercano estás tú, oh Jehová;"
  },
  {
    "id": 25343,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "152",
    "text": "Ya ha mucho que he entendido de tus mandamientos,"
  },
  {
    "id": 25344,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "153",
    "text": "Mira mi aflicción, y líbrame;"
  },
  {
    "id": 25345,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "154",
    "text": "Aboga mi causa, y redímeme:"
  },
  {
    "id": 25346,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "155",
    "text": "Lejos está de los impíos la salud;"
  },
  {
    "id": 25347,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "156",
    "text": "Muchas son tus misericordias, oh Jehová:"
  },
  {
    "id": 25348,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "157",
    "text": "Muchos son mis perseguidores y mis enemigos;"
  },
  {
    "id": 25349,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "158",
    "text": "Veía á los prevaricadores, y 119.158 ver. 136. Sal. 95.10. Ez. 6.9.carcomíame;"
  },
  {
    "id": 25350,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "159",
    "text": "Mira, oh Jehová, que amo tus mandamientos:"
  },
  {
    "id": 25351,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "160",
    "text": "El principio de tu palabra es verdad;"
  },
  {
    "id": 25352,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "161",
    "text": "Príncipes me han perseguido sin causa;"
  },
  {
    "id": 25353,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "162",
    "text": "Gózome yo en tu palabra,"
  },
  {
    "id": 25354,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "163",
    "text": "La mentira aborrezco y abomino:"
  },
  {
    "id": 25355,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "164",
    "text": "Siete veces al día te alabo"
  },
  {
    "id": 25356,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "165",
    "text": "Mucha paz tienen los que aman tu ley;"
  },
  {
    "id": 25357,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "166",
    "text": "Tu salud he esperado, oh Jehová;"
  },
  {
    "id": 25358,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "167",
    "text": "Mi alma ha guardado tus testimonios,"
  },
  {
    "id": 25359,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "168",
    "text": "Guardado he tus mandamientos y tus testimonios;"
  },
  {
    "id": 25360,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "169",
    "text": "Acérquese mi clamor delante de ti, oh Jehová:"
  },
  {
    "id": 25361,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "170",
    "text": "Venga mi oración delante de ti:"
  },
  {
    "id": 25362,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "171",
    "text": "Mis labios rebosarán alabanza,"
  },
  {
    "id": 25363,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "172",
    "text": "Hablará mi lengua tus dichos;"
  },
  {
    "id": 25364,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "173",
    "text": "Sea tu mano en mi socorro;"
  },
  {
    "id": 25365,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "174",
    "text": "Deseado he tu salud, oh Jehová;"
  },
  {
    "id": 25366,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "175",
    "text": "Viva mi alma y alábete;"
  },
  {
    "id": 25367,
    "book": "psalms                                  ",
    "chapter": "119",
    "verse": "176",
    "text": "Yo anduve errante como oveja extraviada; busca á tu siervo;"
  }
]