  {
    "id": 16853,
    "book": "jeremiah                                ",
    "chapter": "48",
    "verse": "41",
    "text": "Kerioth is taken,"
  },
  {
    "id": 16854,
    "book": "jeremiah                                ",
    "chapter": "48",
    "verse": "42",
    "text": "Moab will be destroyed from being a people,"
  },
  {
    "id": 16855,
    "book": "jeremiah                                ",
    "chapter": "48",
    "verse": "43",
    "text": "Terror, the pit, and the snare are on you,"
