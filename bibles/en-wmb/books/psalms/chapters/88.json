    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "15",
    "text": "I am afflicted and ready to die from my youth up."
  },
  {
    "id": 27798,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "16",
    "text": "Your fierce wrath has gone over me."
  },
  {
    "id": 27799,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "17",
