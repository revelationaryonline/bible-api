    "chapter": "88",
    "verse": "1",
    "text": "LORD, the God of my salvation,"
  },
  {
    "id": 27784,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "2",
    "text": "Let my prayer enter into your presence."
  },
  {
    "id": 27785,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "3",
    "text": "For my soul is full of troubles."
  },
  {
    "id": 27786,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "4",
    "text": "I am counted among those who go down into the pit."
  },
  {
    "id": 27787,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "5",
    "text": "set apart among the dead,"
  },
  {
    "id": 27788,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "6",
    "text": "You have laid me in the lowest pit,"
  },
  {
    "id": 27789,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "7",
    "text": "Your wrath lies heavily on me."
  },
  {
    "id": 27790,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "8",
    "text": "You have taken my friends from me."
  },
  {
    "id": 27791,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "9",
    "text": "My eyes are dim from grief."
  },
  {
    "id": 27792,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "10",
    "text": "Do you show wonders to the dead?"
  },
  {
    "id": 27793,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "11",
    "text": "Is your loving kindness declared in the grave?"
  },
  {
    "id": 27794,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "12",
    "text": "Are your wonders made known in the dark?"
  },
  {
    "id": 27795,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "13",
    "text": "But to you, LORD, I have cried."
  },
  {
    "id": 27796,
    "book": "psalms                                  ",
    "chapter": "88",
    "verse": "14",
    "text": "LORD, why do you reject my soul?"
